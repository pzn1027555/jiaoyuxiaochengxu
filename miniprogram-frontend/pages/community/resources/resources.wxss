.res-page{ background: linear-gradient(0, #ffffff, #F5FFF2); min-height:100vh }
.list{ padding: 24rpx }
.item{ display:flex; background:#fff; border-radius:16rpx; padding:20rpx; margin-bottom:20rpx; }
.thumb{ width:200rpx; height:140rpx; border-radius:12rpx; background:#eee; margin-right:16rpx }
.i-right{ flex:1; display:flex; flex-direction:column; position: relative; }
.title{ color:#183314; font-size:30rpx; font-weight:700 }
.desc{ color:#889a88; font-size:24rpx; margin-top:10rpx }
.btn-download{ position:absolute; right:0; bottom:0; background: transparent; color: rgba(84, 178, 71, 1); border: 1rpx solid rgba(84, 178, 71, 1); border-radius: 999px; padding: 0 28rpx; line-height: 56rpx; height: 56rpx; }
.load-more, .load-end{ text-align:center; color:#889a88; padding: 24rpx 0 }

