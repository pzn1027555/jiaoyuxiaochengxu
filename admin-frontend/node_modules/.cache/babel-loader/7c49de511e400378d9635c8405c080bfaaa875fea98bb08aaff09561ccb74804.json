{"ast":null,"code":"import Layout from '@/components/Layout.vue';\nexport default {\n  name: 'Teachers',\n  components: {\n    Layout\n  },\n  data() {\n    return {\n      loading: false,\n      teachers: [],\n      selectedTeachers: [],\n      stats: {\n        totalTeachers: 0,\n        pendingReview: 0,\n        approved: 0,\n        activeTeachers: 0\n      },\n      searchForm: {\n        realName: '',\n        teacherNo: '',\n        reviewStatus: null,\n        level: null,\n        contractStatus: null\n      },\n      pagination: {\n        page: 1,\n        size: 20,\n        total: 0\n      }\n    };\n  },\n  created() {\n    this.loadTeachers();\n    this.loadStats();\n  },\n  methods: {\n    async loadTeachers() {\n      this.loading = true;\n      try {\n        // 模拟数据 - 实际项目中调用API\n        const mockTeachers = [{\n          id: 1,\n          teacherNo: 'T001',\n          realName: '张教授',\n          education: '博士',\n          major: '数学教育',\n          teachingAge: 15,\n          level: 4,\n          reviewStatus: 1,\n          contractStatus: 2,\n          commissionRate: 0.18,\n          totalStudents: 156,\n          totalIncome: 85600,\n          rating: 4.8,\n          complaintRate: 0.02,\n          isRecommended: true,\n          isHidden: false,\n          createTime: '2024-01-15 10:30:00'\n        }, {\n          id: 2,\n          teacherNo: 'T002',\n          realName: '李老师',\n          education: '硕士',\n          major: '英语教育',\n          teachingAge: 8,\n          level: 2,\n          reviewStatus: 1,\n          contractStatus: 1,\n          commissionRate: 0.15,\n          totalStudents: 89,\n          totalIncome: 45200,\n          rating: 4.6,\n          complaintRate: 0.03,\n          isRecommended: false,\n          isHidden: false,\n          createTime: '2024-01-16 09:15:00'\n        }, {\n          id: 3,\n          teacherNo: 'T003',\n          realName: '王老师',\n          education: '本科',\n          major: '物理教育',\n          teachingAge: 5,\n          level: 1,\n          reviewStatus: 0,\n          contractStatus: 0,\n          commissionRate: 0.12,\n          totalStudents: 0,\n          totalIncome: 0,\n          rating: 5.0,\n          complaintRate: 0.00,\n          isRecommended: false,\n          isHidden: false,\n          createTime: '2024-01-17 14:20:00'\n        }];\n        this.teachers = mockTeachers;\n        this.pagination.total = mockTeachers.length;\n      } catch (error) {\n        this.$message.error('加载教师列表失败');\n        console.error(error);\n      } finally {\n        this.loading = false;\n      }\n    },\n    loadStats() {\n      // 模拟统计数据\n      this.stats = {\n        totalTeachers: 248,\n        pendingReview: 12,\n        approved: 225,\n        activeTeachers: 186\n      };\n    },\n    handleSearch() {\n      this.pagination.page = 1;\n      this.loadTeachers();\n    },\n    resetSearch() {\n      this.searchForm = {\n        realName: '',\n        teacherNo: '',\n        reviewStatus: null,\n        level: null,\n        contractStatus: null\n      };\n      this.handleSearch();\n    },\n    handleSelectionChange(selection) {\n      this.selectedTeachers = selection;\n    },\n    handleAdd() {\n      this.$message.info('添加教师功能开发中...');\n    },\n    batchApprove() {\n      if (this.selectedTeachers.length === 0) {\n        this.$message.warning('请选择要审核的教师');\n        return;\n      }\n      this.$message.info('批量审核功能开发中...');\n    },\n    batchSendContract() {\n      if (this.selectedTeachers.length === 0) {\n        this.$message.warning('请选择要发送合同的教师');\n        return;\n      }\n      this.$message.info('批量发送合同功能开发中...');\n    },\n    exportTeachers() {\n      this.$message.info('导出功能开发中...');\n    },\n    handleDetail(teacher) {\n      this.$message.info(`查看教师 ${teacher.realName} 详情功能开发中...`);\n    },\n    handleAction(command, teacher) {\n      switch (command) {\n        case 'edit':\n          this.$message.info(`编辑教师 ${teacher.realName} 功能开发中...`);\n          break;\n        case 'review':\n          this.$message.info(`审核教师 ${teacher.realName} 功能开发中...`);\n          break;\n        case 'contract':\n          this.$message.info(`发送合同给 ${teacher.realName} 功能开发中...`);\n          break;\n        case 'commission':\n          this.$message.info(`调整 ${teacher.realName} 佣金功能开发中...`);\n          break;\n        case 'hide':\n          this.handleHideTeacher(teacher, true);\n          break;\n        case 'show':\n          this.handleHideTeacher(teacher, false);\n          break;\n        case 'delete':\n          this.handleDelete(teacher);\n          break;\n      }\n    },\n    handleRecommendChange(teacher) {\n      this.$message.success(`${teacher.isRecommended ? '已推荐' : '取消推荐'}教师 ${teacher.realName}`);\n    },\n    handleHideTeacher(teacher, isHidden) {\n      teacher.isHidden = isHidden;\n      this.$message.success(`${isHidden ? '已雪藏' : '已取消雪藏'}教师 ${teacher.realName}`);\n    },\n    handleDelete(teacher) {\n      this.$confirm(`确定要删除教师 ${teacher.realName} 吗？此操作不可恢复！`, '警告', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'error'\n      }).then(() => {\n        const index = this.teachers.findIndex(t => t.id === teacher.id);\n        if (index > -1) {\n          this.teachers.splice(index, 1);\n          this.pagination.total--;\n        }\n        this.$message.success('删除成功');\n      }).catch(() => {\n        // 取消操作\n      });\n    },\n    handleSizeChange(size) {\n      this.pagination.size = size;\n      this.loadTeachers();\n    },\n    handleCurrentChange(page) {\n      this.pagination.page = page;\n      this.loadTeachers();\n    },\n    // 工具方法\n    getLevelText(level) {\n      const levelMap = {\n        1: '初级',\n        2: '中级',\n        3: '高级',\n        4: '专家'\n      };\n      return levelMap[level] || '未知';\n    },\n    getLevelTagType(level) {\n      const typeMap = {\n        1: 'info',\n        2: 'warning',\n        3: 'success',\n        4: 'danger'\n      };\n      return typeMap[level] || 'info';\n    },\n    getReviewStatusText(status) {\n      const statusMap = {\n        0: '待审核',\n        1: '已通过',\n        2: '已拒绝'\n      };\n      return statusMap[status] || '未知';\n    },\n    getReviewStatusTagType(status) {\n      const typeMap = {\n        0: 'warning',\n        1: 'success',\n        2: 'danger'\n      };\n      return typeMap[status] || 'info';\n    },\n    getContractStatusText(status) {\n      const statusMap = {\n        0: '未发送',\n        1: '已发送',\n        2: '已签署'\n      };\n      return statusMap[status] || '未知';\n    },\n    getContractStatusTagType(status) {\n      const typeMap = {\n        0: 'info',\n        1: 'warning',\n        2: 'success'\n      };\n      return typeMap[status] || 'info';\n    },\n    formatDate(dateStr) {\n      if (!dateStr) return '-';\n      return dateStr.split(' ')[0];\n    }\n  }\n};","map":{"version":3,"names":["Layout","name","components","data","loading","teachers","selectedTeachers","stats","totalTeachers","pendingReview","approved","activeTeachers","searchForm","realName","teacherNo","reviewStatus","level","contractStatus","pagination","page","size","total","created","loadTeachers","loadStats","methods","mockTeachers","id","education","major","teachingAge","commissionRate","totalStudents","totalIncome","rating","complaintRate","isRecommended","isHidden","createTime","length","error","$message","console","handleSearch","resetSearch","handleSelectionChange","selection","handleAdd","info","batchApprove","warning","batchSendContract","exportTeachers","handleDetail","teacher","handleAction","command","handleHideTeacher","handleDelete","handleRecommendChange","success","$confirm","confirmButtonText","cancelButtonText","type","then","index","findIndex","t","splice","catch","handleSizeChange","handleCurrentChange","getLevelText","levelMap","getLevelTagType","typeMap","getReviewStatusText","status","statusMap","getReviewStatusTagType","getContractStatusText","getContractStatusTagType","formatDate","dateStr","split"],"sources":["src/views/education/Teachers.vue"],"sourcesContent":["<template>\r\n  <Layout>\r\n    <div class=\"teachers\">\r\n      <!-- 页面标题 -->\r\n      <div class=\"page-header\">\r\n        <h2 class=\"page-title\">\r\n          <i class=\"el-icon-user education-primary\" />\r\n          教师管理\r\n        </h2>\r\n        <p class=\"page-description\">管理平台教师信息，包括审核、等级调整、佣金设置等</p>\r\n      </div>\r\n      \r\n      <!-- 统计卡片 -->\r\n      <el-row :gutter=\"20\" class=\"stats-row\">\r\n        <el-col :xs=\"12\" :sm=\"6\" :md=\"6\" :lg=\"6\" :xl=\"6\">\r\n          <div class=\"stat-card total-card\">\r\n            <div class=\"stat-icon\">\r\n              <i class=\"el-icon-user\" />\r\n            </div>\r\n            <div class=\"stat-content\">\r\n              <div class=\"stat-number\">{{ stats.totalTeachers }}</div>\r\n              <div class=\"stat-label\">教师总数</div>\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n        \r\n        <el-col :xs=\"12\" :sm=\"6\" :md=\"6\" :lg=\"6\" :xl=\"6\">\r\n          <div class=\"stat-card pending-card\">\r\n            <div class=\"stat-icon\">\r\n              <i class=\"el-icon-time\" />\r\n            </div>\r\n            <div class=\"stat-content\">\r\n              <div class=\"stat-number\">{{ stats.pendingReview }}</div>\r\n              <div class=\"stat-label\">待审核</div>\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n        \r\n        <el-col :xs=\"12\" :sm=\"6\" :md=\"6\" :lg=\"6\" :xl=\"6\">\r\n          <div class=\"stat-card approved-card\">\r\n            <div class=\"stat-icon\">\r\n              <i class=\"el-icon-check\" />\r\n            </div>\r\n            <div class=\"stat-content\">\r\n              <div class=\"stat-number\">{{ stats.approved }}</div>\r\n              <div class=\"stat-label\">已通过</div>\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n        \r\n        <el-col :xs=\"12\" :sm=\"6\" :md=\"6\" :lg=\"6\" :xl=\"6\">\r\n          <div class=\"stat-card active-card\">\r\n            <div class=\"stat-icon\">\r\n              <i class=\"el-icon-connection\" />\r\n            </div>\r\n            <div class=\"stat-content\">\r\n              <div class=\"stat-number\">{{ stats.activeTeachers }}</div>\r\n              <div class=\"stat-label\">活跃教师</div>\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n      \r\n      <!-- 搜索和筛选 -->\r\n      <div class=\"search-section\">\r\n        <el-card class=\"search-card\">\r\n          <el-form :inline=\"true\" :model=\"searchForm\" class=\"search-form\">\r\n            <el-form-item label=\"教师姓名\">\r\n              <el-input\r\n                v-model=\"searchForm.realName\"\r\n                placeholder=\"请输入教师姓名\"\r\n                prefix-icon=\"el-icon-search\"\r\n                clearable\r\n                style=\"width: 200px\"\r\n              />\r\n            </el-form-item>\r\n            \r\n            <el-form-item label=\"教师编号\">\r\n              <el-input\r\n                v-model=\"searchForm.teacherNo\"\r\n                placeholder=\"请输入教师编号\"\r\n                clearable\r\n                style=\"width: 200px\"\r\n              />\r\n            </el-form-item>\r\n            \r\n            <el-form-item label=\"审核状态\">\r\n              <el-select v-model=\"searchForm.reviewStatus\" placeholder=\"全部状态\" clearable style=\"width: 150px\">\r\n                <el-option label=\"待审核\" :value=\"0\" />\r\n                <el-option label=\"审核通过\" :value=\"1\" />\r\n                <el-option label=\"审核拒绝\" :value=\"2\" />\r\n              </el-select>\r\n            </el-form-item>\r\n            \r\n            <el-form-item label=\"教师等级\">\r\n              <el-select v-model=\"searchForm.level\" placeholder=\"全部等级\" clearable style=\"width: 150px\">\r\n                <el-option label=\"初级教师\" :value=\"1\" />\r\n                <el-option label=\"中级教师\" :value=\"2\" />\r\n                <el-option label=\"高级教师\" :value=\"3\" />\r\n                <el-option label=\"专家教师\" :value=\"4\" />\r\n              </el-select>\r\n            </el-form-item>\r\n            \r\n            <el-form-item label=\"合同状态\">\r\n              <el-select v-model=\"searchForm.contractStatus\" placeholder=\"全部状态\" clearable style=\"width: 150px\">\r\n                <el-option label=\"未发送\" :value=\"0\" />\r\n                <el-option label=\"已发送\" :value=\"1\" />\r\n                <el-option label=\"已签署\" :value=\"2\" />\r\n              </el-select>\r\n            </el-form-item>\r\n            \r\n            <el-form-item>\r\n              <el-button type=\"primary\" @click=\"handleSearch\" icon=\"el-icon-search\">\r\n                搜索\r\n              </el-button>\r\n              <el-button @click=\"resetSearch\" icon=\"el-icon-refresh\">\r\n                重置\r\n              </el-button>\r\n            </el-form-item>\r\n          </el-form>\r\n        </el-card>\r\n      </div>\r\n      \r\n      <!-- 工具栏 -->\r\n      <div class=\"table-toolbar\">\r\n        <div class=\"left\">\r\n          <el-button type=\"primary\" icon=\"el-icon-plus\" @click=\"handleAdd\">\r\n            添加教师\r\n          </el-button>\r\n          <el-button type=\"success\" icon=\"el-icon-check\" @click=\"batchApprove\">\r\n            批量审核\r\n          </el-button>\r\n          <el-button type=\"warning\" icon=\"el-icon-message\" @click=\"batchSendContract\">\r\n            批量发送合同\r\n          </el-button>\r\n          <el-button type=\"info\" icon=\"el-icon-download\" @click=\"exportTeachers\">\r\n            导出数据\r\n          </el-button>\r\n        </div>\r\n        \r\n        <div class=\"right\">\r\n          <el-tooltip content=\"刷新数据\" placement=\"top\">\r\n            <el-button icon=\"el-icon-refresh\" @click=\"loadTeachers\" />\r\n          </el-tooltip>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 教师表格 -->\r\n      <div class=\"table-section\">\r\n        <el-card class=\"table-card\">\r\n          <el-table \r\n            :data=\"teachers\" \r\n            v-loading=\"loading\"\r\n            @selection-change=\"handleSelectionChange\"\r\n            class=\"teacher-table\"\r\n            stripe\r\n          >\r\n            <el-table-column type=\"selection\" width=\"55\" />\r\n            \r\n            <el-table-column prop=\"teacherNo\" label=\"教师编号\" width=\"120\" fixed=\"left\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag size=\"mini\">{{ scope.row.teacherNo }}</el-tag>\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"realName\" label=\"姓名\" width=\"100\" fixed=\"left\" />\r\n            \r\n            <el-table-column prop=\"education\" label=\"学历\" width=\"80\" />\r\n            \r\n            <el-table-column prop=\"major\" label=\"专业\" width=\"120\" show-overflow-tooltip />\r\n            \r\n            <el-table-column prop=\"teachingAge\" label=\"教龄\" width=\"80\">\r\n              <template slot-scope=\"scope\">\r\n                {{ scope.row.teachingAge }}年\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"level\" label=\"等级\" width=\"100\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag \r\n                  :type=\"getLevelTagType(scope.row.level)\" \r\n                  size=\"mini\"\r\n                >\r\n                  {{ getLevelText(scope.row.level) }}\r\n                </el-tag>\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"reviewStatus\" label=\"审核状态\" width=\"100\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag \r\n                  :type=\"getReviewStatusTagType(scope.row.reviewStatus)\" \r\n                  size=\"mini\"\r\n                >\r\n                  {{ getReviewStatusText(scope.row.reviewStatus) }}\r\n                </el-tag>\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"contractStatus\" label=\"合同状态\" width=\"100\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag \r\n                  :type=\"getContractStatusTagType(scope.row.contractStatus)\" \r\n                  size=\"mini\"\r\n                >\r\n                  {{ getContractStatusText(scope.row.contractStatus) }}\r\n                </el-tag>\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"commissionRate\" label=\"佣金比例\" width=\"100\">\r\n              <template slot-scope=\"scope\">\r\n                {{ (scope.row.commissionRate * 100).toFixed(1) }}%\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"totalStudents\" label=\"学生数\" width=\"80\" />\r\n            \r\n            <el-table-column prop=\"totalIncome\" label=\"总收入\" width=\"100\">\r\n              <template slot-scope=\"scope\">\r\n                ¥{{ scope.row.totalIncome.toLocaleString() }}\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"rating\" label=\"评分\" width=\"80\">\r\n              <template slot-scope=\"scope\">\r\n                <el-rate \r\n                  :value=\"scope.row.rating\" \r\n                  disabled \r\n                  show-score \r\n                  text-color=\"#ff9900\"\r\n                  score-template=\"{value}\"\r\n                />\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"complaintRate\" label=\"投诉率\" width=\"80\">\r\n              <template slot-scope=\"scope\">\r\n                <span :class=\"{'high-complaint': scope.row.complaintRate > 0.05}\">\r\n                  {{ (scope.row.complaintRate * 100).toFixed(1) }}%\r\n                </span>\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"isRecommended\" label=\"推荐\" width=\"60\">\r\n              <template slot-scope=\"scope\">\r\n                <el-switch\r\n                  v-model=\"scope.row.isRecommended\"\r\n                  @change=\"handleRecommendChange(scope.row)\"\r\n                  active-color=\"#67c23a\"\r\n                  inactive-color=\"#dcdfe6\"\r\n                />\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"createTime\" label=\"注册时间\" width=\"120\">\r\n              <template slot-scope=\"scope\">\r\n                {{ formatDate(scope.row.createTime) }}\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column label=\"操作\" width=\"200\" fixed=\"right\">\r\n              <template slot-scope=\"scope\">\r\n                <el-button \r\n                  type=\"primary\" \r\n                  size=\"mini\" \r\n                  @click=\"handleDetail(scope.row)\"\r\n                  icon=\"el-icon-view\"\r\n                >\r\n                  详情\r\n                </el-button>\r\n                \r\n                <el-dropdown @command=\"(command) => handleAction(command, scope.row)\">\r\n                  <el-button size=\"mini\" type=\"text\">\r\n                    更多<i class=\"el-icon-arrow-down el-icon--right\" />\r\n                  </el-button>\r\n                  <el-dropdown-menu slot=\"dropdown\">\r\n                    <el-dropdown-item command=\"edit\" icon=\"el-icon-edit\">编辑</el-dropdown-item>\r\n                    <el-dropdown-item command=\"review\" icon=\"el-icon-check\" v-if=\"scope.row.reviewStatus === 0\">审核</el-dropdown-item>\r\n                    <el-dropdown-item command=\"contract\" icon=\"el-icon-document\" v-if=\"scope.row.reviewStatus === 1 && scope.row.contractStatus === 0\">发送合同</el-dropdown-item>\r\n                    <el-dropdown-item command=\"commission\" icon=\"el-icon-coin\">调整佣金</el-dropdown-item>\r\n                    <el-dropdown-item command=\"hide\" icon=\"el-icon-view\" v-if=\"!scope.row.isHidden\">雪藏</el-dropdown-item>\r\n                    <el-dropdown-item command=\"show\" icon=\"el-icon-view\" v-else>取消雪藏</el-dropdown-item>\r\n                    <el-dropdown-item command=\"delete\" icon=\"el-icon-delete\" divided>删除</el-dropdown-item>\r\n                  </el-dropdown-menu>\r\n                </el-dropdown>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n          \r\n          <!-- 分页 -->\r\n          <div class=\"pagination-wrapper\">\r\n            <el-pagination\r\n              @size-change=\"handleSizeChange\"\r\n              @current-change=\"handleCurrentChange\"\r\n              :current-page=\"pagination.page\"\r\n              :page-sizes=\"[10, 20, 50, 100]\"\r\n              :page-size=\"pagination.size\"\r\n              layout=\"total, sizes, prev, pager, next, jumper\"\r\n              :total=\"pagination.total\"\r\n              background\r\n            />\r\n          </div>\r\n        </el-card>\r\n      </div>\r\n    </div>\r\n  </Layout>\r\n</template>\r\n\r\n<script>\r\nimport Layout from '@/components/Layout.vue'\r\n\r\nexport default {\r\n  name: 'Teachers',\r\n  components: {\r\n    Layout\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      teachers: [],\r\n      selectedTeachers: [],\r\n      stats: {\r\n        totalTeachers: 0,\r\n        pendingReview: 0,\r\n        approved: 0,\r\n        activeTeachers: 0\r\n      },\r\n      searchForm: {\r\n        realName: '',\r\n        teacherNo: '',\r\n        reviewStatus: null,\r\n        level: null,\r\n        contractStatus: null\r\n      },\r\n      pagination: {\r\n        page: 1,\r\n        size: 20,\r\n        total: 0\r\n      }\r\n    }\r\n  },\r\n  created() {\r\n    this.loadTeachers()\r\n    this.loadStats()\r\n  },\r\n  methods: {\r\n    async loadTeachers() {\r\n      this.loading = true\r\n      try {\r\n        // 模拟数据 - 实际项目中调用API\r\n        const mockTeachers = [\r\n          {\r\n            id: 1,\r\n            teacherNo: 'T001',\r\n            realName: '张教授',\r\n            education: '博士',\r\n            major: '数学教育',\r\n            teachingAge: 15,\r\n            level: 4,\r\n            reviewStatus: 1,\r\n            contractStatus: 2,\r\n            commissionRate: 0.18,\r\n            totalStudents: 156,\r\n            totalIncome: 85600,\r\n            rating: 4.8,\r\n            complaintRate: 0.02,\r\n            isRecommended: true,\r\n            isHidden: false,\r\n            createTime: '2024-01-15 10:30:00'\r\n          },\r\n          {\r\n            id: 2,\r\n            teacherNo: 'T002',\r\n            realName: '李老师',\r\n            education: '硕士',\r\n            major: '英语教育',\r\n            teachingAge: 8,\r\n            level: 2,\r\n            reviewStatus: 1,\r\n            contractStatus: 1,\r\n            commissionRate: 0.15,\r\n            totalStudents: 89,\r\n            totalIncome: 45200,\r\n            rating: 4.6,\r\n            complaintRate: 0.03,\r\n            isRecommended: false,\r\n            isHidden: false,\r\n            createTime: '2024-01-16 09:15:00'\r\n          },\r\n          {\r\n            id: 3,\r\n            teacherNo: 'T003',\r\n            realName: '王老师',\r\n            education: '本科',\r\n            major: '物理教育',\r\n            teachingAge: 5,\r\n            level: 1,\r\n            reviewStatus: 0,\r\n            contractStatus: 0,\r\n            commissionRate: 0.12,\r\n            totalStudents: 0,\r\n            totalIncome: 0,\r\n            rating: 5.0,\r\n            complaintRate: 0.00,\r\n            isRecommended: false,\r\n            isHidden: false,\r\n   