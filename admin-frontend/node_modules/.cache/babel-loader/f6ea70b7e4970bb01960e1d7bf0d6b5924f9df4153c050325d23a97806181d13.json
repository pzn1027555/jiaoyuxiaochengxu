            :page-size=\"pagination.pageSize\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\"\r\n            :total=\"pagination.total\"\r\n          />\r\n        </div>\r\n      </el-card>\r\n    </div>\r\n\r\n    <!-- 考试表单对话框 -->\r\n    <el-dialog :title=\"dialogTitle\" :visible.sync=\"dialogVisible\" width=\"600px\">\r\n      <el-form :model=\"examForm\" :rules=\"examRules\" ref=\"examForm\" label-width=\"100px\">\r\n        <el-form-item label=\"考试名称\" prop=\"name\">\r\n          <el-input v-model=\"examForm.name\" placeholder=\"请输入考试名称\" />\r\n        </el-form-item>\r\n        <el-form-item label=\"科目\" prop=\"subject\">\r\n          <el-select v-model=\"examForm.subject\" placeholder=\"请选择科目\">\r\n            <el-option label=\"数学\" value=\"数学\" />\r\n            <el-option label=\"语文\" value=\"语文\" />\r\n            <el-option label=\"英语\" value=\"英语\" />\r\n            <el-option label=\"物理\" value=\"物理\" />\r\n            <el-option label=\"化学\" value=\"化学\" />\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item label=\"考试时长\" prop=\"duration\">\r\n          <el-input-number v-model=\"examForm.duration\" :min=\"30\" :max=\"180\" /> 分钟\r\n        </el-form-item>\r\n        <el-form-item label=\"开始时间\" prop=\"startTime\">\r\n          <el-date-picker\r\n            v-model=\"examForm.startTime\"\r\n            type=\"datetime\"\r\n            placeholder=\"选择开始时间\"\r\n            format=\"yyyy-MM-dd HH:mm:ss\"\r\n            value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item label=\"结束时间\" prop=\"endTime\">\r\n          <el-date-picker\r\n            v-model=\"examForm.endTime\"\r\n            type=\"datetime\"\r\n            placeholder=\"选择结束时间\"\r\n            format=\"yyyy-MM-dd HH:mm:ss\"\r\n            value-format=\"yyyy-MM-dd HH:mm:ss\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item label=\"考试说明\" prop=\"description\">\r\n         