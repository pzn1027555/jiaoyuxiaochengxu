{"ast":null,"code":"export default {\n  name: 'Layout',\n  data() {\n    return {\n      componentMap: {\n        'dashboard': () => import('../views/Dashboard.vue'),\n        'my-schedule': () => import('../views/education/MySchedule.vue'),\n        'class-feedback': () => import('../views/education/ClassFeedback.vue'),\n        'one-on-one-feedback': () => import('../views/education/CourseFeedback.vue'),\n        'one-on-one-students': () => import('../views/education/Students.vue'),\n        'teachers': () => import('../views/education/Teachers.vue'),\n        'teacher-review': () => import('../views/education/TeacherReview.vue'),\n        'exams': () => import('../views/education/Exams.vue'),\n        'video-clips': () => import('../views/education/VideoClips.vue'),\n        'leave-request': () => import('../views/system/LeaveRequest.vue'),\n        'questionnaire': () => import('../views/system/Questionnaire.vue'),\n        'statistics': () => import('../views/system/Statistics.vue')\n      }\n    };\n  },\n  computed: {\n    userInfo() {\n      return this.$store.getters.getUser || {\n        username: 'Admin',\n        nickname: '管理员'\n      };\n    },\n    tabs() {\n      return this.$store.getters.getTabs || [];\n    },\n    activeTabKey() {\n      return this.$store.getters.getActiveTabKey || '';\n    },\n    activeMenu() {\n      return this.activeTabKey;\n    },\n    activeTabComponent() {\n      if (!this.activeTabKey || !this.componentMap[this.activeTabKey]) {\n        return null;\n      }\n      return this.componentMap[this.activeTabKey];\n    }\n  },\n  methods: {\n    handleCommand(command) {\n      switch (command) {\n        case 'profile':\n          this.$message.info('个人设置功能开发中...');\n          break;\n        case 'logout':\n          this.handleLogout();\n          break;\n      }\n    },\n    handleLogout() {\n      this.$confirm('确定要退出登录吗？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.$store.dispatch('logout');\n        this.$message.success('退出登录成功');\n        this.$router.push('/login');\n      }).catch(() => {\n        // 取消操作\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","componentMap","dashboard","my-schedule","class-feedback","one-on-one-feedback","one-on-one-students","teachers","teacher-review","exams","video-clips","leave-request","questionnaire","statistics","computed","userInfo","$store","getters","getUser","username","nickname","tabs","getTabs","activeTabKey","getActiveTabKey","activeMenu","activeTabComponent","methods","handleCommand","command","$message","info","handleLogout","$confirm","confirmButtonText","cancelButtonText","type","then","dispatch","success","$router","push","catch"],"sources":["src/components/Layout.vue"],"sourcesContent":["<template>\n  <div class=\"layout-container\">\n    <el-container>\n      <!-- 头部 -->\n      <el-header class=\"layout-header\">\n        <div class=\"header-left\">\n          <h3 class=\"logo\">\n            <i class=\"el-icon-reading\" />\n            教育管理系统平台\n          </h3>\n        </div>\n        \n        <div class=\"header-right\">\n          <div class=\"notification-area\">\n            <el-badge :value=\"3\" class=\"notification-badge\">\n              <el-button type=\"text\" icon=\"el-icon-bell\" class=\"notification-btn\" />\n            </el-badge>\n          </div>\n          <el-dropdown @command=\"handleCommand\" trigger=\"click\">\n            <span class=\"user-info\">\n              <el-avatar \n                :size=\"32\" \n                :src=\"userInfo.avatar\" \n                icon=\"el-icon-user-solid\"\n                class=\"user-avatar\"\n              />\n              <span class=\"username\">欢迎您，{{ userInfo.nickname || userInfo.username || 'George Li' }}</span>\n              <i class=\"el-icon-arrow-down\" />\n            </span>\n            <el-dropdown-menu slot=\"dropdown\">\n              <el-dropdown-item command=\"profile\">\n                <i class=\"el-icon-user\"></i> 个人设置\n              </el-dropdown-item>\n              <el-dropdown-item command=\"logout\" divided>\n                <i class=\"el-icon-switch-button\"></i> 退出登录\n              </el-dropdown-item>\n            </el-dropdown-menu>\n          </el-dropdown>\n        </div>\n      </el-header>\n      \n      <el-container>\n        <!-- 侧边栏 -->\n        <el-aside width=\"220px\" class=\"layout-aside\">\n          <div class=\"sidebar-content\">\n            <el-menu\n              :default-active=\"activeMenu\"\n              :unique-opened=\"false\"\n              :default-openeds=\"['students', 'system']\"\n              class=\"sidebar-menu\"\n            >\n              <!-- 数据研究 -->\n              <el-menu-item index=\"dashboard\" class=\"menu-item\" @click=\"openTab('dashboard', '数学教研')\">\n                <i class=\"el-icon-s-data menu-icon\" />\n                <span slot=\"title\">数学教研</span>\n                <el-badge is-dot class=\"menu-badge\" v-if=\"activeMenu === 'dashboard'\" />\n              </el-menu-item>\n              \n              <!-- 人事行政 -->\n              <el-submenu index=\"teachers\" class=\"menu-submenu\">\n                <template slot=\"title\">\n                  <i class=\"el-icon-user menu-icon\" />\n                  <span>人事行政</span>\n                </template>\n                <el-menu-item index=\"teachers\" class=\"submenu-item\" @click=\"openTab('teachers', '教师列表')\">\n                  <span>教师列表</span>\n                </el-menu-item>\n                <el-menu-item index=\"teacher-review\" class=\"submenu-item\" @click=\"openTab('teacher-review', '教师审核')\">\n                  <span>教师审核</span>\n                </el-menu-item>\n              </el-submenu>\n              \n              <!-- 薪火计划 -->\n              <el-submenu index=\"students\" class=\"menu-submenu\">\n                <template slot=\"title\">\n                  <i class=\"el-icon-user-solid menu-icon\" />\n                  <span>薪火计划</span>\n                </template>\n                <el-menu-item index=\"my-schedule\" class=\"submenu-item\" @click=\"openTab('my-schedule', '我的课表')\">\n                  <span>我的课表</span>\n                </el-menu-item>\n                <el-menu-item index=\"class-feedback\" class=\"submenu-item\" @click=\"openTab('class-feedback', '大课反馈')\">\n                  <span>大课反馈</span>\n                </el-menu-item>\n                <el-menu-item index=\"one-on-one-feedback\" class=\"submenu-item\" @click=\"openTab('one-on-one-feedback', '一对一课程反馈')\">\n                  <span>一对一课程反馈</span>\n                  <el-badge is-dot class=\"menu-badge\" v-if=\"activeMenu === 'one-on-one-feedback'\" />\n                </el-menu-item>\n                <el-menu-item index=\"one-on-one-students\" class=\"submenu-item\" @click=\"openTab('one-on-one-students', '一对一学员')\">\n                  <span>一对一学员</span>\n                </el-menu-item>\n              </el-submenu>\n              \n              <!-- 考试管理 -->\n              <el-menu-item index=\"exams\" class=\"menu-item\" @click=\"openTab('exams', '考试管理')\">\n                <i class=\"el-icon-document menu-icon\" />\n                <span slot=\"title\">考试管理</span>\n              </el-menu-item>\n              \n              <!-- 竞赛课程 -->\n              <el-submenu index=\"courses\" class=\"menu-submenu\">\n                <template slot=\"title\">\n                  <i class=\"el-icon-reading menu-icon\" />\n                  <span>竞赛课程</span>\n                </template>\n                <el-menu-item index=\"video-clips\" class=\"submenu-item\" @click=\"openTab('video-clips', '视频打点')\">\n                  <span>视频打点</span>\n                </el-menu-item>\n              </el-submenu>\n              \n              <!-- 系统管理 -->\n              <el-submenu index=\"system\" class=\"menu-submenu\">\n                <template slot=\"title\">\n                  <i class=\"el-icon-setting menu-icon\" />\n                  <span>系统管理</span>\n                </template>\n                <el-menu-item index=\"leave-request\" class=\"submenu-item\" @click=\"openTab('leave-request', '网课模考评请')\">\n                  <span>网课模考评请</span>\n                </el-menu-item>\n                <el-menu-item index=\"questionnaire\" class=\"submenu-item\" @click=\"openTab('questionnaire', '问卷调查管理')\">\n                  <span>问卷调查管理</span>\n                </el-menu-item>\n                <el-menu-item index=\"statistics\" class=\"submenu-item\" @click=\"openTab('statistics', '教师数据统计')\">\n                  <span>教师数据统计</span>\n                </el-menu-item>\n              </el-submenu>\n            </el-menu>\n          </div>\n        </el-aside>\n        \n        <!-- 主内容区 -->\n        <el-main class=\"layout-main\">\n          <!-- 标签页区域 -->\n          <div class=\"tabs-container\" v-if=\"tabs.length > 0\">\n            <el-tabs\n              v-model=\"activeTabKey\"\n              type=\"card\"\n              closable\n              @tab-remove=\"removeTab\"\n              @tab-click=\"handleTabClick\"\n              class=\"page-tabs\"\n            >\n              <el-tab-pane\n                v-for=\"tab in tabs\"\n                :key=\"tab.key\"\n                :label=\"tab.title\"\n                :name=\"tab.key\"\n              />\n            </el-tabs>\n          </div>\n\n          <!-- 内容区域 -->\n          <div class=\"main-content\">\n            <component \n              v-if=\"activeTabComponent\" \n              :is=\"activeTabComponent\" \n              :key=\"activeTabKey\"\n            />\n            <div v-else class=\"welcome-content\">\n              <div class=\"welcome-box\">\n                <i class=\"el-icon-s-home welcome-icon\" />\n                <h3>欢迎使用教育管理系统</h3>\n                <p>请从左侧菜单选择功能模块开始使用</p>\n              </div>\n            </div>\n          </div>\n        </el-main>\n      </el-container>\n    </el-container>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'Layout',\n  data() {\n    return {\n      componentMap: {\n        'dashboard': () => import('../views/Dashboard.vue'),\n        'my-schedule': () => import('../views/education/MySchedule.vue'),\n        'class-feedback': () => import('../views/education/ClassFeedback.vue'),\n        'one-on-one-feedback': () => import('../views/education/CourseFeedback.vue'),\n        'one-on-one-students': () => import('../views/education/Students.vue'),\n        'teachers': () => import('../views/education/Teachers.vue'),\n        'teacher-review': () => import('../views/education/TeacherReview.vue'),\n        'exams': () => import('../views/education/Exams.vue'),\n        'video-clips': () => import('../views/education/VideoClips.vue'),\n        'leave-request': () => import('../views/system/LeaveRequest.vue'),\n        'questionnaire': () => import('../views/system/Questionnaire.vue'),\n        'statistics': () => import('../views/system/Statistics.vue')\n      }\n    }\n  },\n  computed: {\n    userInfo() {\n      return this.$store.getters.getUser || { username: 'Admin', nickname: '管理员' }\n    },\n    tabs() {\n      return this.$store.getters.getTabs || []\n    },\n    activeTabKey() {\n      return this.$store.getters.getActiveTabKey || ''\n    },\n    activeMenu() {\n      return this.activeTabKey\n    },\n    activeTabComponent() {\n      if (!this.activeTabKey || !this.componentMap[this.activeTabKey]) {\n        return null\n      }\n      return this.componentMap[this.activeTabKey]\n    }\n  },\n  methods: {\n    handleCommand(command) {\n      switch (command) {\n        case 'profile':\n          this.$message.info('个人设置功能开发中...')\n          break\n        case 'logout':\n          this.handleLogout()\n          break\n      }\n    },\n    handleLogout() {\n      this.$confirm('确定要退出登录吗？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.$store.dispatch('logout')\n        this.$message.success('退出登录成功')\n        this.$router.push('/login')\n      }).catch(() => {\n        // 取消操作\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.layout-container {\n  height: 100vh;\n}\n\n.layout-header {\n  background: #ffffff;\n  border-bottom: 1px solid #e8e8e8;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 24px;\n  box-shadow: 0 1px 4px rgba(0, 21, 41, 0.08);\n  height: 60px;\n}\n\n.header-left {\n  display: flex;\n  align-items: center;\n}\n\n.logo {\n  color: #303133;\n  margin: 0;\n  font-size: 18px;\n  font-weight: 600;\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.logo i {\n  font-size: 20px;\n  color: #409eff;\n}\n\n.header-right {\n  display: flex;\n  align-items: center;\n}\n\n.notification-area {\n  margin-right: 16px;\n}\n\n.notification-btn {\n  color: #606266;\n  font-size: 18px;\n  padding: 8px;\n  border-radius: 4px;\n  transition: all 0.3s;\n}\n\n.notification-btn:hover {\n  color: #409eff;\n  background: #ecf5ff;\n}\n\n.notification-badge {\n  margin-right: 8px;\n}\n\n.user-info {\n  display: flex;\n  align-items: center;\n  cursor: pointer;\n  padding: 6px 12px;\n  border-radius: 6px;\n  transition: background-color 0.3s;\n  color: #606266;\n}\n\n.user-info:hover {\n  background-color: #f5f7fa;\n}\n\n.user-avatar {\n  border: 2px solid #e4e7ed;\n}\n\n.username {\n  margin: 0 8px;\n  font-size: 14px;\n  font-weight: 500;\n}\n\n.layout-aside {\n  background: #ffffff;\n  border-right: 1px solid #e8e8e8;\n  box-shadow: 1px 0 4px rgba(0, 21, 41, 0.08);\n}\n\n.sidebar-content {\n  height: 100%;\n  overflow-y: auto;\n  padding: 16px 0;\n}\n\n.sidebar-menu {\n  border: none;\n  background: transparent;\n}\n\n.menu-item {\n  margin: 2px 8px;\n  border-radius: 0;\n  transition: all 0.3s;\n  position: relative;\n}\n\n.menu-item:hover {\n  background: transparent !important;\n  color: #409ef