{"ast":null,"code":"export default {\n  name: 'LeaveRequest',\n  data() {\n    return {\n      loading: false,\n      filterForm: {\n        studentName: '',\n        status: '',\n        dateRange: []\n      },\n      requestsList: [{\n        id: 1,\n        studentName: '陈含',\n        studentId: 'STU001',\n        avatar: '',\n        courseName: 'IGCSE数学',\n        courseType: 'CIE考试局',\n        requestType: 'mock_exam',\n        applyDate: '2024-01-10',\n        examDate: '2024-01-15',\n        reason: '因为身体不适，希望申请模拟考试延期',\n        status: 'pending'\n      }, {\n        id: 2,\n        studentName: '孙小渔',\n        studentId: 'STU002',\n        avatar: '',\n        courseName: 'A-level物理',\n        courseType: 'AS物理',\n        requestType: 'online_class',\n        applyDate: '2024-01-11',\n        examDate: '2024-01-16',\n        reason: '家庭紧急事务，需要请假一周',\n        status: 'approved'\n      }, {\n        id: 3,\n        studentName: 'Bella',\n        studentId: 'STU003',\n        avatar: '',\n        courseName: 'A-level数学',\n        courseType: 'AQA考试局',\n        requestType: 'mock_exam',\n        applyDate: '2024-01-12',\n        examDate: '2024-01-17',\n        reason: '与其他重要考试时间冲突',\n        status: 'rejected'\n      }],\n      selectedRows: [],\n      pagination: {\n        current: 1,\n        size: 20,\n        total: 3\n      }\n    };\n  },\n  methods: {\n    handleSearch() {\n      this.loadRequests();\n    },\n    resetFilter() {\n      this.filterForm = {\n        studentName: '',\n        status: '',\n        dateRange: []\n      };\n      this.loadRequests();\n    },\n    loadRequests() {\n      this.loading = true;\n      setTimeout(() => {\n        this.loading = false;\n      }, 1000);\n    },\n    handleSelectionChange(selection) {\n      this.selectedRows = selection;\n    },\n    handleApproval() {\n      if (this.selectedRows.length === 0) {\n        this.$message.warning('请选择要审批的申请');\n        return;\n      }\n      this.$message.info('批量审批功能开发中...');\n    },\n    approve(row) {\n      this.$confirm(`确定要通过 ${row.studentName} 的申请吗？`, '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'success'\n      }).then(() => {\n        row.status = 'approved';\n        this.$message.success('审批通过');\n      });\n    },\n    reject(row) {\n      this.$prompt('请输入拒绝理由', '拒绝申请', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        inputPattern: /.+/,\n        inputErrorMessage: '请输入拒绝理由'\n      }).then(({\n        value\n      }) => {\n        row.status = 'rejected';\n        row.rejectReason = value;\n        this.$message.success('已拒绝申请');\n      });\n    },\n    viewDetails(row) {\n      this.$message.info(`查看详情：${row.studentName}`);\n    },\n    handleSizeChange(size) {\n      this.pagination.size = size;\n      this.loadRequests();\n    },\n    handleCurrentChange(page) {\n      this.pagination.current = page;\n      this.loadRequests();\n    },\n    getRequestTypeColor(type) {\n      const colors = {\n        mock_exam: 'primary',\n        online_class: 'success',\n        assignment: 'warning'\n      };\n      return colors[type] || 'info';\n    },\n    getRequestTypeName(type) {\n      const names = {\n        mock_exam: '模拟考试',\n        online_class: '网课申请',\n        assignment: '作业延期'\n      };\n      return names[type] || '其他';\n    },\n    getStatusColor(status) {\n      const colors = {\n        pending: 'warning',\n        approved: 'success',\n        rejected: 'danger'\n      };\n      return colors[status] || 'info';\n    },\n    getStatusName(status) {\n      const names = {\n        pending: '待审核',\n        approved: '已通过',\n        rejected: '已拒绝'\n      };\n      return names[status] || '未知';\n    },\n    formatDate(date) {\n      if (!date) return '-';\n      return new Date(date).toLocaleDateString('zh-CN');\n    }\n  },\n  mounted() {\n    this.loadRequests();\n  }\n};","map":{"version":3,"names":["name","data","loading","filterForm","studentName","status","dateRange","requestsList","id","studentId","avatar","courseName","courseType","requestType","applyDate","examDate","reason","selectedRows","pagination","current","size","total","methods","handleSearch","loadRequests","resetFilter","setTimeout","handleSelectionChange","selection","handleApproval","length","$message","warning","info","approve","row","$confirm","confirmButtonText","cancelButtonText","type","then","success","reject","$prompt","inputPattern","inputErrorMessage","value","rejectReason","viewDetails","handleSizeChange","handleCurrentChange","page","getRequestTypeColor","colors","mock_exam","online_class","assignment","getRequestTypeName","names","getStatusColor","pending","approved","rejected","getStatusName","formatDate","date","Date","toLocaleDateString","mounted"],"sources":["src/views/system/LeaveRequest.vue"],"sourcesContent":["<template>\r\n  <div class=\"leave-request-container\">\r\n    <div class=\"page-header\">\r\n      <div class=\"header-content\">\r\n        <h2 class=\"page-title\">网课模考评请</h2>\r\n        <p class=\"page-description\">管理学生的网课模拟考试申请</p>\r\n      </div>\r\n      <div class=\"header-actions\">\r\n        <el-button type=\"primary\" icon=\"el-icon-plus\" @click=\"handleApproval\">\r\n          批量审批\r\n        </el-button>\r\n      </div>\r\n    </div>\r\n    \r\n    <div class=\"page-content\">\r\n      <el-card class=\"content-card\">\r\n        <div class=\"filter-section\">\r\n          <el-form :model=\"filterForm\" inline class=\"filter-form\">\r\n            <el-form-item label=\"学生姓名\">\r\n              <el-input \r\n                v-model=\"filterForm.studentName\" \r\n                placeholder=\"请输入学生姓名\" \r\n                clearable\r\n              />\r\n            </el-form-item>\r\n            <el-form-item label=\"申请状态\">\r\n              <el-select v-model=\"filterForm.status\" placeholder=\"请选择状态\" clearable>\r\n                <el-option label=\"待审核\" value=\"pending\" />\r\n                <el-option label=\"已通过\" value=\"approved\" />\r\n                <el-option label=\"已拒绝\" value=\"rejected\" />\r\n              </el-select>\r\n            </el-form-item>\r\n            <el-form-item label=\"申请日期\">\r\n              <el-date-picker\r\n                v-model=\"filterForm.dateRange\"\r\n                type=\"daterange\"\r\n                format=\"yyyy-MM-dd\"\r\n                value-format=\"yyyy-MM-dd\"\r\n                range-separator=\"至\"\r\n                start-placeholder=\"开始日期\"\r\n                end-placeholder=\"结束日期\"\r\n              />\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <el-button type=\"primary\" @click=\"handleSearch\">查询</el-button>\r\n              <el-button @click=\"resetFilter\">重置</el-button>\r\n            </el-form-item>\r\n          </el-form>\r\n        </div>\r\n        \r\n        <div class=\"table-section\">\r\n          <el-table\r\n            v-loading=\"loading\"\r\n            :data=\"requestsList\"\r\n            style=\"width: 100%\"\r\n            @selection-change=\"handleSelectionChange\"\r\n          >\r\n            <el-table-column type=\"selection\" width=\"55\" />\r\n            \r\n            <el-table-column prop=\"studentName\" label=\"学生姓名\" min-width=\"120\">\r\n              <template slot-scope=\"scope\">\r\n                <div class=\"student-info\">\r\n                  <el-avatar \r\n                    :size=\"32\" \r\n                    :src=\"scope.row.avatar\" \r\n                    icon=\"el-icon-user-solid\"\r\n                  />\r\n                  <div class=\"student-details\">\r\n                    <div class=\"student-name\">{{ scope.row.studentName }}</div>\r\n                    <div class=\"student-id\">学号: {{ scope.row.studentId }}</div>\r\n                  </div>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"course\" label=\"课程信息\" min-width=\"150\">\r\n              <template slot-scope=\"scope\">\r\n                <div class=\"course-info\">\r\n                  <div class=\"course-name\">{{ scope.row.courseName }}</div>\r\n                  <div class=\"course-type\">{{ scope.row.courseType }}</div>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"requestType\" label=\"申请类型\" width=\"120\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag :type=\"getRequestTypeColor(scope.row.requestType)\">\r\n                  {{ getRequestTypeName(scope.row.requestType) }}\r\n                </el-tag>\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"applyDate\" label=\"申请日期\" width=\"120\">\r\n              <template slot-scope=\"scope\">\r\n                {{ formatDate(scope.row.applyDate) }}\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"examDate\" label=\"考试日期\" width=\"120\">\r\n              <template slot-scope=\"scope\">\r\n                {{ formatDate(scope.row.examDate) }}\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"reason\" label=\"申请理由\" min-width=\"200\" show-overflow-tooltip />\r\n            \r\n            <el-table-column prop=\"status\" label=\"状态\" width=\"100\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag :type=\"getStatusColor(scope.row.status)\">\r\n                  {{ getStatusName(scope.row.status) }}\r\n                </el-tag>\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column label=\"操作\" width=\"180\" fixed=\"right\">\r\n              <template slot-scope=\"scope\">\r\n                <el-button \r\n                  v-if=\"scope.row.status === 'pending'\"\r\n                  type=\"text\" \r\n                  size=\"small\" \r\n                  @click=\"approve(scope.row)\"\r\n                >\r\n                  通过\r\n                </el-button>\r\n                <el-button \r\n                  v-if=\"scope.row.status === 'pending'\"\r\n                  type=\"text\" \r\n                  size=\"small\" \r\n                  style=\"color: #f56c6c;\" \r\n                  @click=\"reject(scope.row)\"\r\n                >\r\n                  拒绝\r\n                </el-button>\r\n                <el-button type=\"text\" size=\"small\" @click=\"viewDetails(scope.row)\">\r\n                  详情\r\n                </el-button>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n          \r\n          <div class=\"pagination-section\">\r\n            <el-pagination\r\n              @size-change=\"handleSizeChange\"\r\n              @current-change=\"handleCurrentChange\"\r\n              :current-page=\"pagination.current\"\r\n              :page-sizes=\"[10, 20, 50, 100]\"\r\n              :page-size=\"pagination.size\"\r\n              layout=\"total, sizes, prev, pager, next, jumper\"\r\n              :total=\"pagination.total\"\r\n            />\r\n          </div>\r\n        </div>\r\n      </el-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'LeaveRequest',\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      filterForm: {\r\n        studentName: '',\r\n        status: '',\r\n        dateRange: []\r\n      },\r\n      requestsList: [\r\n        {\r\n          id: 1,\r\n          studentName: '陈含',\r\n          studentId: 'STU001',\r\n          avatar: '',\r\n          courseName: 'IGCSE数学',\r\n          courseType: 'CIE考试局',\r\n          requestType: 'mock_exam',\r\n          applyDate: '2024-01-10',\r\n          examDate: '2024-01-15',\r\n          reason: '因为身体不适，希望申请模拟考试延期',\r\n          status: 'pending'\r\n        },\r\n        {\r\n          id: 2,\r\n          studentName: '孙小渔',\r\n          studentId: 'STU002',\r\n          avatar: '',\r\n          courseName: 'A-level物理',\r\n          courseType: 'AS物理',\r\n          requestType: 'online_class',\r\n          applyDate: '2024-01-11',\r\n          examDate: '2024-01-16',\r\n          reason: '家庭紧急事务，需要请假一周',\r\n          status: 'approved'\r\n        },\r\n        {\r\n          id: 3,\r\n          studentName: 'Bella',\r\n          studentId: 'STU003',\r\n          avatar: '',\r\n          courseName: 'A-level数学',\r\n          courseType: 'AQA考试局',\r\n          requestType: 'mock_exam',\r\n          applyDate: '2024-01-12',\r\n          examDate: '2024-01-17',\r\n          reason: '与其他重要考试时间冲突',\r\n          status: 'rejected'\r\n        }\r\n      ],\r\n      selectedRows: [],\r\n      pagination: {\r\n        current: 1,\r\n        size: 20,\r\n        total: 3\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    handleSearch() {\r\n      this.loadRequests()\r\n    },\r\n    \r\n    resetFilter() {\r\n      this.filterForm = {\r\n        studentName: '',\r\n        status: '',\r\n        dateRange: []\r\n      }\r\n      this.loadRequests()\r\n    },\r\n    \r\n    loadRequests() {\r\n      this.loading = true\r\n      setTimeout(() => {\r\n        this.loading = false\r\n      }, 1000)\r\n    },\r\n    \r\n    handleSelectionChange(selection) {\r\n      this.selectedRows = selection\r\n    },\r\n    \r\n    handleApproval() {\r\n      if (this.selectedRows.length === 0) {\r\n        this.$message.warning('请选择要审批的申请')\r\n        return\r\n      }\r\n      this.$message.info('批量审批功能开发中...')\r\n    },\r\n    \r\n    approve(row) {\r\n      this.$confirm(`确定要通过 ${row.studentName} 的申请吗？`, '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'success'\r\n      }).then(() => {\r\n        row.status = 'approved'\r\n        this.$message.success('审批通过')\r\n      })\r\n    },\r\n    \r\n    reject(row) {\r\n      this.$prompt('请输入拒绝理由', '拒绝申请', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        inputPattern: /.+/,\r\n        inputErrorMessage: '请输入拒绝理由'\r\n      }).then(({ value }) => {\r\n        row.status = 'rejected'\r\n        row.rejectReason = value\r\n        this.$message.success('已拒绝申请')\r\n      })\r\n    },\r\n    \r\n    viewDetails(row) {\r\n      this.$message.info(`查看详情：${row.studentName}`)\r\n    },\r\n    \r\n    handleSizeChange(size) {\r\n      this.pagination.size = size\r\n      this.loadRequests()\r\n    },\r\n    \r\n    handleCurrentChange(page) {\r\n      this.pagination.current = page\r\n      this.loadRequests()\r\n    },\r\n    \r\n    getRequestTypeColor(type) {\r\n      const colors = {\r\n        mock_exam: 'primary',\r\n        online_class: 'success',\r\n        assignment: 'warning'\r\n      }\r\n      return colors[type] || 'info'\r\n    },\r\n    \r\n    getRequestTypeName(type) {\r\n      const names = {\r\n        mock_exam: '模拟考试',\r\n        online_class: '网课申请',\r\n        assignment: '作业延期'\r\n      }\r\n      return names[type] || '其他'\r\n    },\r\n    \r\n    getStatusColor(status) {\r\n      const colors = {\r\n        pending: 'warning',\r\n        approved: 'success',\r\n        rejected: 'danger'\r\n      }\r\n      return colors[status] || 'info'\r\n    },\r\n    \r\n    getStatusName(status) {\r\n      const names = {\r\n        pending: '待审核',\r\n        approved: '已通过',\r\n        rejected: '已拒绝'\r\n      }\r\n      return names[status] || '未知'\r\n    },\r\n    \r\n    formatDate(date) {\r\n      if (!date) return '-'\r\n      return new Date(date).toLocaleDateString('zh-CN')\r\n    }\r\n  },\r\n  \r\n  mounted() {\r\n    this.loadRequests()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.leave-request-container {\r\n  padding: 0;\r\n}\r\n\r\n.page-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: 