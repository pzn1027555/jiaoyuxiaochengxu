{"ast":null,"code":"import CircleButton from '@/components/CircleButton.vue';\nexport default {\n  name: 'Teachers',\n  components: {\n    CircleButton\n  },\n  data() {\n    return {\n      loading: false,\n      teachers: [],\n      selectedTeachers: [],\n      stats: {\n        totalTeachers: 0,\n        pendingReview: 0,\n        approved: 0,\n        activeTeachers: 0\n      },\n      searchForm: {\n        realName: '',\n        teacherNo: '',\n        reviewStatus: null,\n        level: null,\n        contractStatus: null\n      },\n      pagination: {\n        page: 1,\n        size: 20,\n        total: 0\n      }\n    };\n  },\n  created() {\n    this.loadTeachers();\n    this.loadStats();\n  },\n  methods: {\n    async loadTeachers() {\n      this.loading = true;\n      try {\n        // 模拟数据 - 实际项目中调用API\n        const mockTeachers = [{\n          id: 1,\n          teacherNo: 'T001',\n          realName: '张教授',\n          education: '博士',\n          major: '数学教育',\n          teachingAge: 15,\n          level: 4,\n          reviewStatus: 1,\n          contractStatus: 2,\n          commissionRate: 0.18,\n          totalStudents: 156,\n          totalIncome: 85600,\n          rating: 4.8,\n          complaintRate: 0.02,\n          isRecommended: true,\n          isHidden: false,\n          createTime: '2024-01-15 10:30:00'\n        }, {\n          id: 2,\n          teacherNo: 'T002',\n          realName: '李老师',\n          education: '硕士',\n          major: '英语教育',\n          teachingAge: 8,\n          level: 2,\n          reviewStatus: 1,\n          contractStatus: 1,\n          commissionRate: 0.15,\n          totalStudents: 89,\n          totalIncome: 45200,\n          rating: 4.6,\n          complaintRate: 0.03,\n          isRecommended: false,\n          isHidden: false,\n          createTime: '2024-01-16 09:15:00'\n        }, {\n          id: 3,\n          teacherNo: 'T003',\n          realName: '王老师',\n          education: '本科',\n          major: '物理教育',\n          teachingAge: 5,\n          level: 1,\n          reviewStatus: 0,\n          contractStatus: 0,\n          commissionRate: 0.12,\n          totalStudents: 0,\n          totalIncome: 0,\n          rating: 5.0,\n          complaintRate: 0.00,\n          isRecommended: false,\n          isHidden: false,\n          createTime: '2024-01-17 14:20:00'\n        }];\n        this.teachers = mockTeachers;\n        this.pagination.total = mockTeachers.length;\n      } catch (error) {\n        this.$message.error('加载教师列表失败');\n        console.error(error);\n      } finally {\n        this.loading = false;\n      }\n    },\n    loadStats() {\n      // 模拟统计数据\n      this.stats = {\n        totalTeachers: 248,\n        pendingReview: 12,\n        approved: 225,\n        activeTeachers: 186\n      };\n    },\n    handleSearch() {\n      this.pagination.page = 1;\n      this.loadTeachers();\n    },\n    resetSearch() {\n      this.searchForm = {\n        realName: '',\n        teacherNo: '',\n        reviewStatus: null,\n        level: null,\n        contractStatus: null\n      };\n      this.handleSearch();\n    },\n    handleSelectionChange(selection) {\n      this.selectedTeachers = selection;\n    },\n    handleAdd() {\n      this.$message.info('添加教师功能开发中...');\n    },\n    batchApprove() {\n      if (this.selectedTeachers.length === 0) {\n        this.$message.warning('请选择要审核的教师');\n        return;\n      }\n      this.$message.info('批量审核功能开发中...');\n    },\n    batchSendContract() {\n      if (this.selectedTeachers.length === 0) {\n        this.$message.warning('请选择要发送合同的教师');\n        return;\n      }\n      this.$message.info('批量发送合同功能开发中...');\n    },\n    exportTeachers() {\n      this.$message.info('导出功能开发中...');\n    },\n    getActionButtons(teacher) {\n      const actions = [{\n        icon: 'el-icon-view',\n        type: 'primary',\n        tooltip: '查看详情',\n        action: 'view',\n        data: teacher\n      }, {\n        icon: 'el-icon-edit',\n        type: 'warning',\n        tooltip: '编辑',\n        action: 'edit',\n        data: teacher\n      }, {\n        icon: 'el-icon-coin',\n        type: 'success',\n        tooltip: '调整佣金',\n        action: 'commission',\n        data: teacher\n      }];\n      if (teacher.reviewStatus === 0) {\n        actions.push({\n          icon: 'el-icon-check',\n          type: 'success',\n          tooltip: '审核',\n          action: 'review',\n          data: teacher\n        });\n      }\n      if (teacher.reviewStatus === 1 && teacher.contractStatus === 0) {\n        actions.push({\n          icon: 'el-icon-document',\n          type: 'info',\n          tooltip: '发送合同',\n          action: 'contract',\n          data: teacher\n        });\n      }\n      actions.push({\n        icon: 'el-icon-delete',\n        type: 'danger',\n        tooltip: '删除',\n        action: 'delete',\n        data: teacher\n      });\n      return actions;\n    },\n    handleAction(actionData) {\n      const {\n        action,\n        data: teacher\n      } = actionData;\n      switch (action) {\n        case 'view':\n          this.$message.info(`查看教师 ${teacher.realName} 详情功能开发中...`);\n          break;\n        case 'edit':\n          this.$message.info(`编辑教师 ${teacher.realName} 功能开发中...`);\n          break;\n        case 'review':\n          this.$message.info(`审核教师 ${teacher.realName} 功能开发中...`);\n          break;\n        case 'contract':\n          this.$message.info(`发送合同给 ${teacher.realName} 功能开发中...`);\n          break;\n        case 'commission':\n          this.$message.info(`调整 ${teacher.realName} 佣金功能开发中...`);\n          break;\n        case 'delete':\n          this.handleDelete(teacher);\n          break;\n      }\n    },\n    handleRecommendChange(teacher) {\n      this.$message.success(`${teacher.isRecommended ? '已推荐' : '取消推荐'}教师 ${teacher.realName}`);\n    },\n    handleHideTeacher(teacher, isHidden) {\n      teacher.isHidden = isHidden;\n      this.$message.success(`${isHidden ? '已雪藏' : '已取消雪藏'}教师 ${teacher.realName}`);\n    },\n    handleDelete(teacher) {\n      this.$confirm(`确定要删除教师 ${teacher.realName} 吗？此操作不可恢复！`, '警告', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'error'\n      }).then(() => {\n        const index = this.teachers.findIndex(t => t.id === teacher.id);\n        if (index > -1) {\n          this.teachers.splice(index, 1);\n          this.pagination.total--;\n        }\n        this.$message.success('删除成功');\n      }).catch(() => {\n        // 取消操作\n      });\n    },\n    handleSizeChange(size) {\n      this.pagination.size = size;\n      this.loadTeachers();\n    },\n    handleCurrentChange(page) {\n      this.pagination.page = page;\n      this.loadTeachers();\n    },\n    // 工具方法\n    getLevelText(level) {\n      const levelMap = {\n        1: '初级',\n        2: '中级',\n        3: '高级',\n        4: '专家'\n      };\n      return levelMap[level] || '未知';\n    },\n    getLevelTagType(level) {\n      const typeMap = {\n        1: 'info',\n        2: 'warning',\n        3: 'success',\n        4: 'danger'\n      };\n      return typeMap[level] || 'info';\n    },\n    getReviewStatusText(status) {\n      const statusMap = {\n        0: '待审核',\n        1: '已通过',\n        2: '已拒绝'\n      };\n      return statusMap[status] || '未知';\n    },\n    getReviewStatusTagType(status) {\n      const typeMap = {\n        0: 'warning',\n        1: 'success',\n        2: 'danger'\n      };\n      return typeMap[status] || 'info';\n    },\n    getContractStatusText(status) {\n      const statusMap = {\n        0: '未发送',\n        1: '已发送',\n        2: '已签署'\n      };\n      return statusMap[status] || '未知';\n    },\n    getContractStatusTagType(status) {\n      const typeMap = {\n        0: 'info',\n        1: 'warning',\n        2: 'success'\n      };\n      return typeMap[status] || 'info';\n    },\n    formatDate(dateStr) {\n      if (!dateStr) return '-';\n      return dateStr.split(' ')[0];\n    }\n  }\n};","map":{"version":3,"names":["CircleButton","name","components","data","loading","teachers","selectedTeachers","stats","totalTeachers","pendingReview","approved","activeTeachers","searchForm","realName","teacherNo","reviewStatus","level","contractStatus","pagination","page","size","total","created","loadTeachers","loadStats","methods","mockTeachers","id","education","major","teachingAge","commissionRate","totalStudents","totalIncome","rating","complaintRate","isRecommended","isHidden","createTime","length","error","$message","console","handleSearch","resetSearch","handleSelectionChange","selection","handleAdd","info","batchApprove","warning","batchSendContract","exportTeachers","getActionButtons","teacher","actions","icon","type","tooltip","action","push","handleAction","actionData","handleDelete","handleRecommendChange","success","handleHideTeacher","$confirm","confirmButtonText","cancelButtonText","then","index","findIndex","t","splice","catch","handleSizeChange","handleCurrentChange","getLevelText","levelMap","getLevelTagType","typeMap","getReviewStatusText","status","statusMap","getReviewStatusTagType","getContractStatusText","getContractStatusTagType","formatDate","dateStr","split"],"sources":["src/views/education/Teachers.vue"],"sourcesContent":["<template>\n  <div class=\"teachers-container\">\n    <!-- 页面头部 -->\n    <div class=\"page-header\">\n      <div class=\"header-content\">\n        <h1 class=\"page-title\">\n          <i class=\"el-icon-user\" />\n          教师管理\n        </h1>\n        <p class=\"page-description\">管理平台教师信息，包括审核、等级调整、佣金设置等</p>\n      </div>\n    </div>\n      \n    <!-- 统计卡片 -->\n    <div class=\"stats-grid\">\n      <div class=\"stat-card\">\n        <div class=\"stat-icon\" style=\"background: linear-gradient(135deg, #409eff 0%, #337ecc 100%)\">\n          <i class=\"el-icon-user\" />\n        </div>\n        <div class=\"stat-content\">\n          <div class=\"stat-value\">{{ stats.totalTeachers }}</div>\n          <div class=\"stat-label\">教师总数</div>\n        </div>\n      </div>\n      \n      <div class=\"stat-card\">\n        <div class=\"stat-icon\" style=\"background: linear-gradient(135deg, #e6a23c 0%, #d48806 100%)\">\n          <i class=\"el-icon-time\" />\n        </div>\n        <div class=\"stat-content\">\n          <div class=\"stat-value\">{{ stats.pendingReview }}</div>\n          <div class=\"stat-label\">待审核</div>\n        </div>\n      </div>\n      \n      <div class=\"stat-card\">\n        <div class=\"stat-icon\" style=\"background: linear-gradient(135deg, #67c23a 0%, #52a832 100%)\">\n          <i class=\"el-icon-check\" />\n        </div>\n        <div class=\"stat-content\">\n          <div class=\"stat-value\">{{ stats.approved }}</div>\n          <div class=\"stat-label\">已通过</div>\n        </div>\n      </div>\n      \n      <div class=\"stat-card\">\n        <div class=\"stat-icon\" style=\"background: linear-gradient(135deg, #409eff 0%, #337ecc 100%)\">\n          <i class=\"el-icon-connection\" />\n        </div>\n        <div class=\"stat-content\">\n          <div class=\"stat-value\">{{ stats.activeTeachers }}</div>\n          <div class=\"stat-label\">活跃教师</div>\n        </div>\n      </div>\n    </div>\n      \n    <!-- 搜索和筛选 -->\n    <div class=\"search-container\">\n      <el-form :model=\"searchForm\" class=\"search-form\" inline>\n        <el-form-item label=\"教师姓名\">\n          <el-input\n            v-model=\"searchForm.realName\"\n            placeholder=\"请输入教师姓名\"\n            clearable\n            style=\"width: 180px\"\n          />\n        </el-form-item>\n        \n        <el-form-item label=\"教师编号\">\n          <el-input\n            v-model=\"searchForm.teacherNo\"\n            placeholder=\"请输入教师编号\"\n            clearable\n            style=\"width: 180px\"\n          />\n        </el-form-item>\n        \n        <el-form-item label=\"审核状态\">\n          <el-select v-model=\"searchForm.reviewStatus\" placeholder=\"全部状态\" clearable style=\"width: 140px\">\n            <el-option label=\"待审核\" :value=\"0\" />\n            <el-option label=\"审核通过\" :value=\"1\" />\n            <el-option label=\"审核拒绝\" :value=\"2\" />\n          </el-select>\n        </el-form-item>\n        \n        <el-form-item label=\"教师等级\">\n          <el-select v-model=\"searchForm.level\" placeholder=\"全部等级\" clearable style=\"width: 140px\">\n            <el-option label=\"初级教师\" :value=\"1\" />\n            <el-option label=\"中级教师\" :value=\"2\" />\n            <el-option label=\"高级教师\" :value=\"3\" />\n            <el-option label=\"专家教师\" :value=\"4\" />\n          </el-select>\n        </el-form-item>\n        \n        <el-form-item label=\"合同状态\">\n          <el-select v-model=\"searchForm.contractStatus\" placeholder=\"全部状态\" clearable style=\"width: 140px\">\n            <el-option label=\"未发送\" :value=\"0\" />\n            <el-option label=\"已发送\" :value=\"1\" />\n            <el-option label=\"已签署\" :value=\"2\" />\n          </el-select>\n        </el-form-item>\n        \n        <el-form-item>\n          <el-button type=\"primary\" @click=\"handleSearch\" icon=\"el-icon-search\">\n            搜索\n          </el-button>\n          <el-button @click=\"resetSearch\" icon=\"el-icon-refresh\">\n            重置\n          </el-button>\n        </el-form-item>\n      </el-form>\n    </div>\n      \n    <!-- 工具栏 -->\n    <div class=\"table-toolbar\">\n      <div class=\"left\">\n        <el-button type=\"primary\" icon=\"el-icon-plus\" @click=\"handleAdd\">\n          添加教师\n        </el-button>\n        <el-button type=\"success\" icon=\"el-icon-check\" @click=\"batchApprove\">\n          批量审核\n        </el-button>\n        <el-button type=\"warning\" icon=\"el-icon-message\" @click=\"batchSendContract\">\n          批量发送合同\n        </el-button>\n        <el-button type=\"info\" icon=\"el-icon-download\" @click=\"exportTeachers\">\n          导出数据\n        </el-button>\n      </div>\n      \n      <div class=\"right\">\n        <el-tooltip content=\"刷新数据\" placement=\"top\">\n          <el-button icon=\"el-icon-refresh\" @click=\"loadTeachers\" />\n        </el-tooltip>\n      </div>\n    </div>\n      \n    <!-- 教师表格 -->\n    <div class=\"table-container\">\n      <el-table \n            :data=\"teachers\" \n            v-loading=\"loading\"\n            @selection-change=\"handleSelectionChange\"\n            class=\"teacher-table\"\n            stripe\n          >\n            <el-table-column type=\"selection\" width=\"55\" />\n            \n            <el-table-column prop=\"teacherNo\" label=\"教师编号\" width=\"120\" fixed=\"left\">\n              <template slot-scope=\"scope\">\n                <el-tag size=\"mini\">{{ scope.row.teacherNo }}</el-tag>\n              </template>\n            </el-table-column>\n            \n            <el-table-column prop=\"realName\" label=\"姓名\" width=\"100\" fixed=\"left\" />\n            \n            <el-table-column prop=\"education\" label=\"学历\" width=\"80\" />\n            \n            <el-table-column prop=\"major\" label=\"专业\" width=\"120\" show-overflow-tooltip />\n            \n            <el-table-column prop=\"teachingAge\" label=\"教龄\" width=\"80\">\n              <template slot-scope=\"scope\">\n                {{ scope.row.teachingAge }}年\n              </template>\n            </el-table-column>\n            \n            <el-table-column prop=\"level\" label=\"等级\" width=\"100\">\n              <template slot-scope=\"scope\">\n                <el-tag \n                  :type=\"getLevelTagType(scope.row.level)\" \n                  size=\"mini\"\n                >\n                  {{ getLevelText(scope.row.level) }}\n                </el-tag>\n              </template>\n            </el-table-column>\n            \n            <el-table-column prop=\"reviewStatus\" label=\"审核状态\" width=\"100\">\n              <template slot-scope=\"scope\">\n                <el-tag \n                  :type=\"getReviewStatusTagType(scope.row.reviewStatus)\" \n                  size=\"mini\"\n                >\n                  {{ getReviewStatusText(scope.row.reviewStatus) }}\n                </el-tag>\n              </template>\n            </el-table-column>\n            \n            <el-table-column prop=\"contractStatus\" label=\"合同状态\" width=\"100\">\n              <template slot-scope=\"scope\">\n                <el-tag \n                  :type=\"getContractStatusTagType(scope.row.contractStatus)\" \n                  size=\"mini\"\n                >\n                  {{ getContractStatusText(scope.row.contractStatus) }}\n                </el-tag>\n              </template>\n            </el-table-column>\n            \n            <el-table-column prop=\"commissionRate\" label=\"佣金比例\" width=\"100\">\n              <template slot-scope=\"scope\">\n                {{ (scope.row.commissionRate * 100).toFixed(1) }}%\n              </template>\n            </el-table-column>\n            \n            <el-table-column prop=\"totalStudents\" label=\"学生数\" width=\"80\" />\n            \n            <el-table-column prop=\"totalIncome\" label=\"总收入\" width=\"100\">\n              <template slot-scope=\"scope\">\n                ¥{{ scope.row.totalIncome.toLocaleString() }}\n              </template>\n            </el-table-column>\n            \n            <el-table-column prop=\"rating\" label=\"评分\" width=\"80\">\n              <template slot-scope=\"scope\">\n                <el-rate \n                  :value=\"scope.row.rating\" \n                  disabled \n                  show-score \n                  text-color=\"#ff9900\"\n                  score-template=\"{value}\"\n                />\n              </template>\n            </el-table-column>\n            \n            <el-table-column prop=\"complaintRate\" label=\"投诉率\" width=\"80\">\n              <template slot-scope=\"scope\">\n                <span :class=\"{'high-complaint': scope.row.complaintRate > 0.05}\">\n                  {{ (scope.row.complaintRate * 100).toFixed(1) }}%\n                </span>\n              </template>\n            </el-table-column>\n            \n            <el-table-column prop=\"isRecommended\" label=\"推荐\" width=\"60\">\n              <template slot-scope=\"scope\">\n                <el-switch\n                  v-model=\"scope.row.isRecommended\"\n                  @change=\"handleRecommendChange(scope.row)\"\n                  active-color=\"#67c23a\"\n                  inactive-color=\"#dcdfe6\"\n                />\n              </template>\n            </el-table-column>\n            \n            <el-table-column prop=\"createTime\" label=\"注册时间\" width=\"120\">\n              <template slot-scope=\"scope\">\n                {{ formatDate(scope.row.createTime) }}\n              </template>\n            </el-table-column>\n            \n            <el-table-column label=\"操作\" width=\"240\" fixed=\"right\" align=\"center\">\n              <template slot-scope=\"scope\">\n                <CircleButton \n                  :actions=\"getActionButtons(scope.row)\"\n                  @action-click=\"handleAction\"\n                />\n              </template>\n            </el-table-column>\n          </el-table>\n          \n      <!-- 分页 -->\n      <div class=\"pagination-wrapper\">\n        <div class=\"pagination-info\">\n          显示 1 - {{ teachers.length }} 条，共 {{ pagination.total }} 条\n        </div>\n        <el-pagination\n          @size-change=\"handleSizeChange\"\n          @current-change=\"handleCurrentChange\"\n          :current-page=\"pagination.page\"\n          :page-sizes=\"[10, 20, 50, 100]\"\n          :page-size=\"pagination.size\"\n          layout=\"prev, pager, next\"\n          :total=\"pagination.total\"\n          background\n        />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport CircleButton from '@/components/CircleButton.vue'\n\nexport default {\n  name: 'Teachers',\n  components: {\n    CircleButton\n  },\n  data() {\n    return {\n      loading: false,\n      teachers: [],\n      selectedTeachers: [],\n      stats: {\n        totalTeachers: 0,\n        pendingReview: 0,\n        approved: 0,\n        activeTeachers: 0\n      },\n      searchForm: {\n        realName: '',\n        teacherNo: '',\n        reviewStatus: null,\n        level: null,\n        contractStatus: null\n      },\n      pagination: {\n        page: 1,\n        size: 20,\n        total: 0\n      }\n    }\n  },\n  created() {\n    this.loadTeachers()\n    this.loadStats()\n  },\n  methods: {\n    async loadTeachers() {\n      this.loading = true\n      try {\n        // 模拟数据 - 实际项目中调用API\n        const mockTeachers = [\n          {\n            id: 1,\n            teacherNo: 'T001',\n            realName: '张教授',\n            education: '博士',\n            major: '数学教育',\n            teachingAge: 15,\n            level: 4,\n            reviewStatus: 1,\n            contractStatus: 2,\n            commissionRate: 0.18,\n            totalStudents: 156,\n            totalIncome: 85600,\n            rating: 4.8,\n            complaintRate: 0.02,\n            isRecommended: true,\n            isHidden: false,\n            createTime: '2024-01-15 10:30:00'\n          },\n          {\n            id: 2,\n            teacherNo: 'T002',\n            realName: '李老师',\n            education: '硕士',\n            major: '英语教育',\n            teachingAge: 8,\n            level: 2,\n            reviewStatus: 1,\n            contractStatus: 1,\n            commissionRate: 0.15,\n            totalStudents: 89,\n            totalIncome: 45200,\n            rating: 4.6,\n            complaintRate: 0.03,\n            isRecommended: false,\n            isHidden: false,\n            createTime: '2024-01-16 09:15:00'\n          },\n          {\n            id: 3,\n            teacherNo: 'T003',\n            realName: '王老师',\n            education: '本科',\n            major: '物理教育',\n            teachingAge: 5,\n            level: 1,\n            reviewStatus: 0,\n            contractStatus: 0,\n            commissionRate: 0.12,\n            totalStudents: 0,\n            totalIncome: 0,\n            rating: 5.0,\n            complaintRate: 0.00,\n            isRecommended: false,\n            isHidden: false,\n            createTime: '2024-01-17 14:20:00'\n          }\n        ]\n        \n        this.teachers = mockTeachers\n        this.pagination.total = mockTeachers.length\n        \n      } catch (error) {\n        this.$message.error('加载教师列表失败')\n        console.error(error)\n      } finally {\n        this.loading = false\n      }\n    },\n    \n    loadStats() {\n      // 模拟统计数据\n      this.stats = {\n        totalTeachers: 248,\n        pendingReview: 12,\n        approved: 225,\n        activeTeachers: 186\n      }\n    },\n    \n    handleSearch() {\n      this.pagination.page = 1\n      this.loadTeachers()\n    },\n    \n    resetSearch() {\n      this.searchForm = {\n        realName: '',\n        teacherNo: '',\n        reviewStatus: null,\n        level: null,\n        contractStatus: null\n      }\n      this.handleSearch()\n    },\n    \n    handleSelectionChange(selection) {\n      this.selectedTeachers = selection\n    },\n    \n    handleAdd() {\n      this.$message.info('添加教师功能开发中...')\n    },\n    \n    batchApprove() {\n      if (this.selectedTeachers.length === 0) {\n        this.$message.warning('请选择要审核的教师')\n        return\n      }\n      this.$message.info('批量审核功能开发中...')\n    },\n    \n    batchSendContract() {\n      if (this.selectedTeachers.length === 0) {\n        this.$message.warning('请选择要发送合同的教师')\n        return\n      }\n      this.$message.info('批量发送合同功能开发中...')\n    },\n    \n    exportTeachers() {\n      this.$message.info('导出功能开发中...')\n    },\n    \n    getActionButtons(teacher) {\n      const actions = [\n        { icon: 'el-icon-view', type: 'primary', tooltip: '查看详情', action: 'view', data: teacher },\n        { icon: 'el-icon-ed