{"ast":null,"code":"import Layout from '@/components/Layout.vue';\nimport { teacherApi } from '@/api/teacher';\nexport default {\n  name: 'TeacherReview',\n  components: {\n    Layout\n  },\n  data() {\n    return {\n      loading: false,\n      teacherList: [],\n      selectedTeachers: [],\n      statistics: {\n        pendingReview: 0,\n        approved: 0,\n        rejected: 0,\n        todayReview: 0\n      },\n      searchForm: {\n        realName: '',\n        reviewStatus: null,\n        dateRange: null\n      },\n      pagination: {\n        page: 1,\n        size: 10,\n        total: 0\n      },\n      detailDialogVisible: false,\n      currentTeacher: null,\n      reviewForm: {\n        reviewStatus: 1,\n        reviewReason: ''\n      },\n      submitting: false\n    };\n  },\n  created() {\n    this.loadTeachers();\n    this.loadStatistics();\n  },\n  methods: {\n    async loadTeachers() {\n      this.loading = true;\n      try {\n        const params = {\n          page: this.pagination.page,\n          size: this.pagination.size,\n          ...this.searchForm\n        };\n\n        // 处理日期范围\n        if (this.searchForm.dateRange && this.searchForm.dateRange.length === 2) {\n          params.startDate = this.searchForm.dateRange[0];\n          params.endDate = this.searchForm.dateRange[1];\n        }\n        const response = await teacherApi.getTeachersByPage(params);\n        if (response.code === 200) {\n          this.teacherList = response.data.teachers || [];\n          this.pagination.total = response.data.total || 0;\n        }\n      } catch (error) {\n        this.$message.error('加载教师列表失败');\n        console.error(error);\n      } finally {\n        this.loading = false;\n      }\n    },\n    async loadStatistics() {\n      try {\n        const response = await teacherApi.getStatistics();\n        if (response.code === 200) {\n          this.statistics = {\n            pendingReview: response.data.pendingReview || 0,\n            approved: response.data.approved || 0,\n            rejected: response.data.rejected || 0,\n            todayReview: response.data.todayReview || 0\n          };\n        }\n      } catch (error) {\n        console.error('加载统计数据失败:', error);\n      }\n    },\n    handleSearch() {\n      this.pagination.page = 1;\n      this.loadTeachers();\n    },\n    handleReset() {\n      this.$refs.searchForm.resetFields();\n      this.pagination.page = 1;\n      this.loadTeachers();\n    },\n    handleSizeChange(val) {\n      this.pagination.size = val;\n      this.pagination.page = 1;\n      this.loadTeachers();\n    },\n    handleCurrentChange(val) {\n      this.pagination.page = val;\n      this.loadTeachers();\n    },\n    handleSelectionChange(selection) {\n      this.selectedTeachers = selection;\n    },\n    handleViewDetail(teacher) {\n      this.currentTeacher = teacher;\n      this.reviewForm = {\n        reviewStatus: 1,\n        reviewReason: ''\n      };\n      this.detailDialogVisible = true;\n    },\n    handleDetailClose() {\n      this.currentTeacher = null;\n      this.reviewForm = {\n        reviewStatus: 1,\n        reviewReason: ''\n      };\n    },\n    async handleApprove(teacher) {\n      try {\n        const response = await teacherApi.reviewTeacher(teacher.id, {\n          reviewStatus: 1,\n          reviewReason: '审核通过'\n        });\n        if (response.code === 200) {\n          this.$message.success('审核通过成功');\n          this.loadTeachers();\n          this.loadStatistics();\n        }\n      } catch (error) {\n        this.$message.error('审核失败');\n      }\n    },\n    async handleReject(teacher) {\n      this.$prompt('请输入拒绝原因', '拒绝审核', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        inputType: 'textarea',\n        inputValidator: value => {\n          if (!value || value.trim() === '') {\n            return '请输入拒绝原因';\n          }\n          return true;\n        }\n      }).then(async ({\n        value\n      }) => {\n        try {\n          const response = await teacherApi.reviewTeacher(teacher.id, {\n            reviewStatus: 2,\n            reviewReason: value\n          });\n          if (response.code === 200) {\n            this.$message.success('审核拒绝成功');\n            this.loadTeachers();\n            this.loadStatistics();\n          }\n        } catch (error) {\n          this.$message.error('审核失败');\n        }\n      }).catch(() => {});\n    },\n    async handleReviewSubmit() {\n      if (!this.reviewForm.reviewStatus) {\n        this.$message.warning('请选择审核结果');\n        return;\n      }\n      this.submitting = true;\n      try {\n        const response = await teacherApi.reviewTeacher(this.currentTeacher.id, this.reviewForm);\n        if (response.code === 200) {\n          this.$message.success('审核成功');\n          this.detailDialogVisible = false;\n          this.loadTeachers();\n          this.loadStatistics();\n        }\n      } catch (error) {\n        this.$message.error('审核失败');\n      } finally {\n        this.submitting = false;\n      }\n    },\n    async batchApprove() {\n      if (this.selectedTeachers.length === 0) {\n        this.$message.warning('请选择要审核的教师');\n        return;\n      }\n      const pendingTeachers = this.selectedTeachers.filter(t => t.reviewStatus === 0);\n      if (pendingTeachers.length === 0) {\n        this.$message.warning('选中的教师中没有待审核的');\n        return;\n      }\n      try {\n        const ids = pendingTeachers.map(t => t.id);\n        const response = await teacherApi.batchApprove(ids);\n        if (response.code === 200) {\n          this.$message.success('批量审核通过成功');\n          this.loadTeachers();\n          this.loadStatistics();\n        }\n      } catch (error) {\n        this.$message.error('批量审核失败');\n      }\n    },\n    batchReject() {\n      if (this.selectedTeachers.length === 0) {\n        this.$message.warning('请选择要审核的教师');\n        return;\n      }\n      const pendingTeachers = this.selectedTeachers.filter(t => t.reviewStatus === 0);\n      if (pendingTeachers.length === 0) {\n        this.$message.warning('选中的教师中没有待审核的');\n        return;\n      }\n      this.$prompt('请输入拒绝原因', '批量拒绝', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        inputType: 'textarea',\n        inputValidator: value => {\n          if (!value || value.trim() === '') {\n            return '请输入拒绝原因';\n          }\n          return true;\n        }\n      }).then(async ({\n        value\n      }) => {\n        try {\n          const promises = pendingTeachers.map(teacher => teacherApi.reviewTeacher(teacher.id, {\n            reviewStatus: 2,\n            reviewReason: value\n          }));\n          await Promise.all(promises);\n          this.$message.success('批量审核拒绝成功');\n          this.loadTeachers();\n          this.loadStatistics();\n        } catch (error) {\n          this.$message.error('批量审核失败');\n        }\n      }).catch(() => {});\n    },\n    getReviewStatusText(status) {\n      const statusMap = {\n        0: '待审核',\n        1: '已通过',\n        2: '已拒绝'\n      };\n      return statusMap[status] || '未知';\n    }\n  }\n};","map":{"version":3,"names":["Layout","teacherApi","name","components","data","loading","teacherList","selectedTeachers","statistics","pendingReview","approved","rejected","todayReview","searchForm","realName","reviewStatus","dateRange","pagination","page","size","total","detailDialogVisible","currentTeacher","reviewForm","reviewReason","submitting","created","loadTeachers","loadStatistics","methods","params","length","startDate","endDate","response","getTeachersByPage","code","teachers","error","$message","console","getStatistics","handleSearch","handleReset","$refs","resetFields","handleSizeChange","val","handleCurrentChange","handleSelectionChange","selection","handleViewDetail","teacher","handleDetailClose","handleApprove","reviewTeacher","id","success","handleReject","$prompt","confirmButtonText","cancelButtonText","inputType","inputValidator","value","trim","then","catch","handleReviewSubmit","warning","batchApprove","pendingTeachers","filter","t","ids","map","batchReject","promises","Promise","all","getReviewStatusText","status","statusMap"],"sources":["src/views/education/TeacherReview.vue"],"sourcesContent":["<template>\r\n  <Layout>\r\n    <div class=\"teacher-review-container\">\r\n      <!-- 页面头部 -->\r\n      <div class=\"page-header\">\r\n        <div class=\"header-content\">\r\n          <h1 class=\"page-title\">\r\n            <i class=\"el-icon-s-check\"></i>\r\n            教师审核\r\n          </h1>\r\n          <p class=\"page-description\">审核教师申请，管理教师认证状态</p>\r\n        </div>\r\n        <div class=\"header-actions\">\r\n          <el-button type=\"primary\" icon=\"el-icon-check\" @click=\"batchApprove\">\r\n            批量通过\r\n          </el-button>\r\n          <el-button type=\"danger\" icon=\"el-icon-close\" @click=\"batchReject\">\r\n            批量拒绝\r\n          </el-button>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 统计卡片 -->\r\n      <div class=\"stats-cards\">\r\n        <div class=\"stat-card pending\">\r\n          <div class=\"stat-icon\">\r\n            <i class=\"el-icon-time\"></i>\r\n          </div>\r\n          <div class=\"stat-content\">\r\n            <div class=\"stat-value\">{{ statistics.pendingReview || 0 }}</div>\r\n            <div class=\"stat-label\">待审核</div>\r\n          </div>\r\n        </div>\r\n        <div class=\"stat-card approved\">\r\n          <div class=\"stat-icon\">\r\n            <i class=\"el-icon-success\"></i>\r\n          </div>\r\n          <div class=\"stat-content\">\r\n            <div class=\"stat-value\">{{ statistics.approved || 0 }}</div>\r\n            <div class=\"stat-label\">已通过</div>\r\n          </div>\r\n        </div>\r\n        <div class=\"stat-card rejected\">\r\n          <div class=\"stat-icon\">\r\n            <i class=\"el-icon-error\"></i>\r\n          </div>\r\n          <div class=\"stat-content\">\r\n            <div class=\"stat-value\">{{ statistics.rejected || 0 }}</div>\r\n            <div class=\"stat-label\">已拒绝</div>\r\n          </div>\r\n        </div>\r\n        <div class=\"stat-card today\">\r\n          <div class=\"stat-icon\">\r\n            <i class=\"el-icon-date\"></i>\r\n          </div>\r\n          <div class=\"stat-content\">\r\n            <div class=\"stat-value\">{{ statistics.todayReview || 0 }}</div>\r\n            <div class=\"stat-label\">今日审核</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 搜索筛选 -->\r\n      <div class=\"search-section\">\r\n        <el-form ref=\"searchForm\" :model=\"searchForm\" inline class=\"search-form\">\r\n          <el-form-item label=\"教师姓名\" prop=\"realName\">\r\n            <el-input \r\n              v-model=\"searchForm.realName\" \r\n              placeholder=\"请输入教师姓名\"\r\n              clearable\r\n              style=\"width: 200px\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item label=\"审核状态\" prop=\"reviewStatus\">\r\n            <el-select \r\n              v-model=\"searchForm.reviewStatus\" \r\n              placeholder=\"请选择审核状态\"\r\n              clearable\r\n              style=\"width: 200px\"\r\n            >\r\n              <el-option label=\"待审核\" :value=\"0\" />\r\n              <el-option label=\"审核通过\" :value=\"1\" />\r\n              <el-option label=\"审核拒绝\" :value=\"2\" />\r\n            </el-select>\r\n          </el-form-item>\r\n          <el-form-item label=\"申请时间\" prop=\"dateRange\">\r\n            <el-date-picker\r\n              v-model=\"searchForm.dateRange\"\r\n              type=\"daterange\"\r\n              range-separator=\"至\"\r\n              start-placeholder=\"开始日期\"\r\n              end-placeholder=\"结束日期\"\r\n              format=\"yyyy-MM-dd\"\r\n              value-format=\"yyyy-MM-dd\"\r\n              style=\"width: 240px\"\r\n            />\r\n          </el-form-item>\r\n          <el-form-item>\r\n            <el-button type=\"primary\" icon=\"el-icon-search\" @click=\"handleSearch\">\r\n              搜索\r\n            </el-button>\r\n            <el-button icon=\"el-icon-refresh\" @click=\"handleReset\">\r\n              重置\r\n            </el-button>\r\n          </el-form-item>\r\n        </el-form>\r\n      </div>\r\n\r\n      <!-- 教师列表 -->\r\n      <div class=\"table-section\">\r\n        <el-table\r\n          v-loading=\"loading\"\r\n          :data=\"teacherList\"\r\n          stripe\r\n          border\r\n          @selection-change=\"handleSelectionChange\"\r\n          style=\"width: 100%\"\r\n        >\r\n          <el-table-column type=\"selection\" width=\"55\" />\r\n          <el-table-column prop=\"teacherNo\" label=\"教师编号\" width=\"120\" />\r\n          <el-table-column prop=\"realName\" label=\"姓名\" width=\"100\" />\r\n          <el-table-column prop=\"idCard\" label=\"身份证号\" width=\"160\" show-overflow-tooltip />\r\n          <el-table-column prop=\"education\" label=\"学历\" width=\"80\" />\r\n          <el-table-column prop=\"major\" label=\"专业\" width=\"120\" show-overflow-tooltip />\r\n          <el-table-column prop=\"teachingAge\" label=\"教龄\" width=\"80\">\r\n            <template slot-scope=\"scope\">\r\n              {{ scope.row.teachingAge }}年\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"reviewStatus\" label=\"审核状态\" width=\"100\">\r\n            <template slot-scope=\"scope\">\r\n              <el-tag\r\n                :type=\"scope.row.reviewStatus === 0 ? 'warning' : scope.row.reviewStatus === 1 ? 'success' : 'danger'\"\r\n              >\r\n                {{ getReviewStatusText(scope.row.reviewStatus) }}\r\n              </el-tag>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"createTime\" label=\"申请时间\" width=\"160\" />\r\n          <el-table-column label=\"操作\" width=\"200\" fixed=\"right\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button \r\n                size=\"small\" \r\n                type=\"primary\" \r\n                @click=\"handleViewDetail(scope.row)\"\r\n                icon=\"el-icon-view\"\r\n              >\r\n                详情\r\n              </el-button>\r\n              <el-button \r\n                v-if=\"scope.row.reviewStatus === 0\"\r\n                size=\"small\" \r\n                type=\"success\" \r\n                @click=\"handleApprove(scope.row)\"\r\n                icon=\"el-icon-check\"\r\n              >\r\n                通过\r\n              </el-button>\r\n              <el-button \r\n                v-if=\"scope.row.reviewStatus === 0\"\r\n                size=\"small\" \r\n                type=\"danger\" \r\n                @click=\"handleReject(scope.row)\"\r\n                icon=\"el-icon-close\"\r\n              >\r\n                拒绝\r\n              </el-button>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n\r\n        <!-- 分页 -->\r\n        <div class=\"pagination-wrapper\">\r\n          <el-pagination\r\n            @size-change=\"handleSizeChange\"\r\n            @current-change=\"handleCurrentChange\"\r\n            :current-page=\"pagination.page\"\r\n            :page-sizes=\"[10, 20, 50, 100]\"\r\n            :page-size=\"pagination.size\"\r\n            layout=\"total, sizes, prev, pager, next, jumper\"\r\n            :total=\"pagination.total\"\r\n          />\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 审核详情对话框 -->\r\n      <el-dialog\r\n        title=\"教师详情\"\r\n        :visible.sync=\"detailDialogVisible\"\r\n        width=\"800px\"\r\n        @close=\"handleDetailClose\"\r\n      >\r\n        <div v-if=\"currentTeacher\" class=\"teacher-detail\">\r\n          <!-- 基本信息 -->\r\n          <el-card class=\"detail-card\">\r\n            <div slot=\"header\">\r\n              <span>基本信息</span>\r\n            </div>\r\n            <el-row :gutter=\"20\">\r\n              <el-col :span=\"12\">\r\n                <div class=\"detail-item\">\r\n                  <label>教师编号：</label>\r\n                  <span>{{ currentTeacher.teacherNo }}</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <div class=\"detail-item\">\r\n                  <label>真实姓名：</label>\r\n                  <span>{{ currentTeacher.realName }}</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <div class=\"detail-item\">\r\n                  <label>身份证号：</label>\r\n                  <span>{{ currentTeacher.idCard }}</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <div class=\"detail-item\">\r\n                  <label>教龄：</label>\r\n                  <span>{{ currentTeacher.teachingAge }}年</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <div class=\"detail-item\">\r\n                  <label>学历：</label>\r\n                  <span>{{ currentTeacher.education }}</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"12\">\r\n                <div class=\"detail-item\">\r\n                  <label>专业：</label>\r\n                  <span>{{ currentTeacher.major }}</span>\r\n                </div>\r\n              </el-col>\r\n              <el-col :span=\"24\">\r\n                <div class=\"detail-item\">\r\n                  <label>自我介绍：</label>\r\n                  <p>{{ currentTeacher.introduction || '暂无' }}</p>\r\n                </div>\r\n              </el-col>\r\n            </el-row>\r\n          </el-card>\r\n\r\n          <!-- 审核操作 -->\r\n          <el-card v-if=\"currentTeacher.reviewStatus === 0\" class=\"detail-card\">\r\n            <div slot=\"header\">\r\n              <span>审核操作</span>\r\n            </div>\r\n            <el-form ref=\"reviewForm\" :model=\"reviewForm\" label-width=\"80px\">\r\n              <el-form-item label=\"审核结果\" prop=\"reviewStatus\">\r\n                <el-radio-group v-model=\"reviewForm.reviewStatus\">\r\n                  <el-radio :label=\"1\">通过</el-radio>\r\n                  <el-radio :label=\"2\">拒绝</el-radio>\r\n                </el-radio-group>\r\n              </el-form-item>\r\n              <el-form-item label=\"审核意见\" prop=\"reviewReason\">\r\n                <el-input\r\n                  v-model=\"reviewForm.reviewReason\"\r\n                  type=\"textarea\"\r\n                  :rows=\"4\"\r\n                  placeholder=\"请输入审核意见\"\r\n                />\r\n              </el-form-item>\r\n            </el-form>\r\n          </el-card>\r\n        </div>\r\n\r\n        <div slot=\"footer\" class=\"dialog-footer\">\r\n          <el-button @click=\"detailDialogVisible = false\">取消</el-button>\r\n          <el-button \r\n            v-if=\"currentTeacher && currentTeacher.reviewStatus === 0\"\r\n            type=\"primary\" \r\n            @click=\"handleReviewSubmit\"\r\n            :loading=\"submitting\"\r\n          >\r\n            提交审核\r\n          </el-button>\r\n        </div>\r\n      </el-dialog>\r\n    </div>\r\n  </Layout>\r\n</template>\r\n\r\n<script>\r\nimport Layout from '@/components/Layout.vue'\r\nimport { teacherApi } from '@/api/teacher'\r\n\r\nexport default {\r\n  name: 'TeacherReview',\r\n  components: {\r\n    Layout\r\n  },\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      teacherList: [],\r\n      selectedTeachers: [],\r\n      statistics: {\r\n        pendingReview: 0,\r\n        approved: 0,\r\n        rejected: 0,\r\n        todayReview: 0\r\n      },\r\n      searchForm: {\r\n        realName: '',\r\n        reviewStatus: null,\r\n        dateRange: null\r\n      },\r\n      pagination: {\r\n        page: 1,\r\n        size: 10,\r\n        total: 0\r\n      },\r\n      detailDialogVisible: false,\r\n      currentTeacher: null,\r\n      reviewForm: {\r\n        reviewStatus: 1,\r\n        reviewReason: ''\r\n      },\r\n      submitting: false\r\n    }\r\n  },\r\n  created() {\r\n    this.loadTeachers()\r\n    this.loadStatistics()\r\n  },\r\n  methods: {\r\n    async loadTeachers() {\r\n      this.loading = true\r\n      try {\r\n        const params = {\r\n          page: this.pagination.page,\r\n          size: this.pagination.size,\r\n          ...this.searchForm\r\n        }\r\n        \r\n        // 处理日期范围\r\n        if (this.searchForm.dateRange && this.searchForm.dateRange.length === 2) {\r\n          params.startDate = this.searchForm.dateRange[0]\r\n          params.endDate = this.searchForm.dateRange[1]\r\n        }\r\n        \r\n        const response = await teacherApi.getTeachersByPage(params)\r\n        if (response.code === 200) {\r\n          this.teacherList = response.data.teachers || []\r\n          this.pagination.total = response.data.total || 0\r\n        }\r\n      } catch (error) {\r\n        this.$message.error('加载教师列表失败')\r\n        console.error(error)\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    },\r\n    \r\n    async loadStatistics() {\r\n      try {\r\n        const response = await teacherApi.getStatistics()\r\n        if (response.code === 200) {\r\n          this.statistics = {\r\n            pendingReview: response.data.pendingReview || 0,\r\n            approved: response.data.approved || 0,\r\n            rejected: response.data.rejected || 0,\r\n            todayReview: response.data.todayReview || 0\r\n          }\r\n        }\r\n      } catch (error) {\r\n        console.error('加载统计数据失败:', error)\r\n      }\r\n    },\r\n    \r\n    handleSearch() {\r\n      this.pagination.page = 1\r\n      this.loadTeachers()\r\n    },\r\n    \r\n    handleReset() {\r\n      this.$refs.searchForm.resetFields()\r\n      this.pagination.page = 1\r\n      this.loadTeachers()\r\n    },\r\n    \r\n    handleSizeChange(val) {\r\n      this.pagination.size = val\r\n      this.pagination.page = 1\r\n      this.loadTeachers()\r\n    },\r\n    \r\n    handleCurrentChange(val) {\r\n      this.pagination.page = val\r\n      this.loadTeachers()\r\n    },\r\n    \r\n    handleSelectionChange(selection) {\r\n      this.selectedTeachers = selection\r\n    },\r\n    \r\n    handleViewDetail(teacher) {\r\n      this.currentTeacher = teacher\r\n      this.reviewForm = {\r\n        reviewStatus: 1,\r\n        reviewReason: ''\r\n      }\r\n      this.detailDialogVisible = true\r\n    },\r\n    \r\n    handleDetailClose() {\r\n      this.currentTeacher = null\r\n      this.reviewForm = {\r\n        reviewStatus: 1,\r\n        reviewReason: ''\r\n      }\r\n    },\r\n    \r\n    async handleApprove(teacher) {\r\n      try {\r\n        const response = await teacherApi.reviewTeacher(teacher.id, {\r\n          reviewStatus: 1,\r\n          reviewReason: '审核通过'\r\n        })\r\n        if (response.code === 200) {\r\n          this.$message.success('审核通过成功')\r\n          this.loadTeachers()\r\n          this.loadStatistics()\r\n        }\r\n      } catch (error) {\r\n        this.$message.error('审核失败')\r\n      }\r\n    },\r\n    \r\n    async handleReject(teacher) {\r\n      this.$prompt('请输入拒绝原因', '拒绝审核', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        inputType: 'textarea',\r\n        inputValidator: (value) => {\r\n          if (!value || value.trim() === '') {\r\n            return '请输入拒绝原因'\r\n          }\r\n          return true\r\n        }\r\n      }).then(async ({ value }) => {\r\n        try {\r\n          const response = await teacherApi.reviewTeacher(teacher.id, {\r\n            reviewStatus: 2,\r\n            reviewReason: value\r\n          })\r\n          if (response.code === 200) {\r\n            this.$message.success('审核拒绝成功')\r\n            this.loadTeachers()\r\n            this.loadStatistics()\r\n          }\r\n        } catch (error) {\r\n          this.$me