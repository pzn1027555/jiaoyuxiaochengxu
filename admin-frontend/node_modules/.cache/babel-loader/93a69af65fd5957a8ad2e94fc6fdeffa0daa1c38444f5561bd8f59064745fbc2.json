{"ast":null,"code":"import Layout from '@/components/Layout.vue';\nexport default {\n  name: 'Dashboard',\n  components: {\n    Layout\n  },\n  data() {\n    return {\n      stats: {\n        todayRecharge: 0,\n        yesterdayRecharge: 0,\n        weekRecharge: 0,\n        monthRecharge: 0,\n        todayConsume: 0,\n        yesterdayConsume: 0,\n        weekConsume: 0,\n        monthConsume: 0,\n        todayLessons: 0,\n        todayStudents: 0\n      },\n      recentActivity: []\n    };\n  },\n  created() {\n    this.loadDashboardData();\n  },\n  methods: {\n    async loadDashboardData() {\n      try {\n        // 模拟加载数据\n        this.stats = {\n          todayRecharge: 0,\n          yesterdayRecharge: 8,\n          weekRecharge: 25,\n          monthRecharge: 156,\n          todayConsume: 0,\n          yesterdayConsume: 12,\n          weekConsume: 45,\n          monthConsume: 203,\n          todayLessons: 0,\n          todayStudents: 0\n        };\n        this.recentActivity = [{\n          time: '2024-01-15 14:30',\n          user: '张老师',\n          action: '添加课程',\n          target: '高中数学基础课程',\n          status: '成功'\n        }, {\n          time: '2024-01-15 13:15',\n          user: '李老师',\n          action: '学员充值',\n          target: '王小明 - 20课时',\n          status: '成功'\n        }, {\n          time: '2024-01-15 12:00',\n          user: '王老师',\n          action: '课程消课',\n          target: '初中英语提高班',\n          status: '成功'\n        }];\n      } catch (error) {\n        console.error('加载仪表盘数据失败:', error);\n      }\n    },\n    viewStats() {\n      this.$message.info('跳转到统计页面');\n    },\n    addConsumption() {\n      this.$message.info('打开消课功能');\n    },\n    studentRecharge() {\n      this.$message.info('打开充值功能');\n    }\n  }\n};","map":{"version":3,"names":["Layout","name","components","data","stats","todayRecharge","yesterdayRecharge","weekRecharge","monthRecharge","todayConsume","yesterdayConsume","weekConsume","monthConsume","todayLessons","todayStudents","recentActivity","created","loadDashboardData","methods","time","user","action","target","status","error","console","viewStats","$message","info","addConsumption","studentRecharge"],"sources":["src/views/Dashboard.vue"],"sourcesContent":["<template>\r\n  <Layout>\r\n    <div class=\"dashboard\">\r\n      <!-- 顶部欢迎区域 -->\r\n      <div class=\"welcome-section\">\r\n        <div class=\"welcome-content\">\r\n          <h1 class=\"welcome-title\">智慧教育管理系统</h1>\r\n          <p class=\"welcome-subtitle\">教育管理专属平台，高效管理教学资源</p>\r\n        </div>\r\n        <div class=\"welcome-actions\">\r\n          <el-button type=\"primary\" icon=\"el-icon-plus\" size=\"medium\">\r\n            快速添加\r\n          </el-button>\r\n        </div>\r\n      </div>\r\n      \r\n      <!-- 数据统计卡片 -->\r\n      <el-row :gutter=\"20\" class=\"stats-section\">\r\n        <el-col :xs=\"24\" :sm=\"8\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"stats-card today-recharge\">\r\n            <div class=\"stats-content\">\r\n              <div class=\"stats-title\">今日充值 (课时)</div>\r\n              <div class=\"stats-number\">{{ stats.todayRecharge }}</div>\r\n              <div class=\"stats-details\">\r\n                <span>昨日 {{ stats.yesterdayRecharge }}</span>\r\n                <span>本周 {{ stats.weekRecharge }}</span>\r\n                <span>本月 {{ stats.monthRecharge }}</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n        \r\n        <el-col :xs=\"24\" :sm=\"8\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"stats-card today-consume\">\r\n            <div class=\"stats-content\">\r\n              <div class=\"stats-title\">今日消课 (课时)</div>\r\n              <div class=\"stats-number\">{{ stats.todayConsume }}</div>\r\n              <div class=\"stats-details\">\r\n                <span>昨日 {{ stats.yesterdayConsume }}</span>\r\n                <span>本周 {{ stats.weekConsume }}</span>\r\n                <span>本月 {{ stats.monthConsume }}</span>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n        \r\n        <el-col :xs=\"24\" :sm=\"8\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"stats-card today-lessons\">\r\n            <div class=\"stats-content\">\r\n              <div class=\"stats-title\">今日课次</div>\r\n              <div class=\"stats-number\">{{ stats.todayLessons }}</div>\r\n              <div class=\"stats-details\">\r\n                <span>今日课程人数 {{ stats.todayStudents }}</span>\r\n              </div>\r\n              <div class=\"stats-link\">\r\n                <a href=\"#\" @click.prevent=\"viewStats\">查看统计</a>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n      \r\n      <!-- 功能模块卡片 -->\r\n      <div class=\"function-modules\">\r\n        <el-row :gutter=\"30\">\r\n          <el-col :xs=\"12\" :sm=\"6\" :md=\"6\" :lg=\"6\" :xl=\"6\">\r\n            <div class=\"module-card\" @click=\"$router.push('/courses')\">\r\n              <div class=\"module-icon course-icon\">\r\n                <i class=\"el-icon-video-camera\"></i>\r\n              </div>\r\n              <div class=\"module-title\">上课查询</div>\r\n              <div class=\"module-desc\">快速查看今日课程</div>\r\n            </div>\r\n          </el-col>\r\n          \r\n          <el-col :xs=\"12\" :sm=\"6\" :md=\"6\" :lg=\"6\" :xl=\"6\">\r\n            <div class=\"module-card\" @click=\"$router.push('/students')\">\r\n              <div class=\"module-icon record-icon\">\r\n                <i class=\"el-icon-document\"></i>\r\n              </div>\r\n              <div class=\"module-title\">记录查询</div>\r\n              <div class=\"module-desc\">学员课程，充值记录</div>\r\n            </div>\r\n          </el-col>\r\n          \r\n          <el-col :xs=\"12\" :sm=\"6\" :md=\"6\" :lg=\"6\" :xl=\"6\">\r\n            <div class=\"module-card\" @click=\"addConsumption\">\r\n              <div class=\"module-icon consume-icon\">\r\n                <i class=\"el-icon-sold-out\"></i>\r\n              </div>\r\n              <div class=\"module-title\">添加消课</div>\r\n              <div class=\"module-desc\">简易操作学员消课</div>\r\n            </div>\r\n          </el-col>\r\n          \r\n          <el-col :xs=\"12\" :sm=\"6\" :md=\"6\" :lg=\"6\" :xl=\"6\">\r\n            <div class=\"module-card\" @click=\"studentRecharge\">\r\n              <div class=\"module-icon recharge-icon\">\r\n                <i class=\"el-icon-coin\"></i>\r\n              </div>\r\n              <div class=\"module-title\">学员充值</div>\r\n              <div class=\"module-desc\">充值课时快捷入口</div>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n      \r\n      <!-- 消息推送和使用帮助 -->\r\n      <el-row :gutter=\"20\" class=\"bottom-section\">\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n          <div class=\"message-card\">\r\n            <h3>消息推送</h3>\r\n            \r\n            <div class=\"message-item\">\r\n              <div class=\"message-content\">\r\n                <div class=\"message-title\">课程取消通知</div>\r\n                <div class=\"message-desc\">课程取消后会推送给家长，让家长第一时间收到取消通知</div>\r\n              </div>\r\n              <div class=\"message-action\">\r\n                <el-button type=\"text\" icon=\"el-icon-more\"></el-button>\r\n              </div>\r\n            </div>\r\n            \r\n            <div class=\"message-item\">\r\n              <div class=\"message-content\">\r\n                <div class=\"message-title\">课程调整通知</div>\r\n                <div class=\"message-desc\">课程临时调整后会推送给家长，让家长第一时间收到调整通知</div>\r\n              </div>\r\n              <div class=\"message-action\">\r\n                <el-button type=\"text\" icon=\"el-icon-more\"></el-button>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n        \r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"12\" :lg=\"12\" :xl=\"12\">\r\n          <div class=\"help-card\">\r\n            <h3>使用帮助</h3>\r\n            <div class=\"help-banner\">\r\n              <div class=\"help-content\">\r\n                <h4>简易消课流程</h4>\r\n                <p>新手课业必看</p>\r\n              </div>\r\n              <div class=\"help-video\">\r\n                <el-button type=\"primary\" icon=\"el-icon-video-play\">观看视频</el-button>\r\n              </div>\r\n            </div>\r\n            \r\n            <div class=\"help-links\">\r\n              <div class=\"help-link-item\">\r\n                <i class=\"el-icon-reading\"></i>\r\n                <a href=\"#\">课消宝（极简版）教师端操作手册</a>\r\n              </div>\r\n              <div class=\"help-link-item\">\r\n                <i class=\"el-icon-school\"></i>\r\n                <a href=\"#\">课消宝学员端操作手册</a>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n      \r\n      <!-- 最近活动 -->\r\n      <div class=\"recent-activity\">\r\n        <h3>最近活动</h3>\r\n        <el-table :data=\"recentActivity\" size=\"small\" style=\"width: 100%\">\r\n          <el-table-column prop=\"time\" label=\"时间\" width=\"150\" />\r\n          <el-table-column prop=\"user\" label=\"操作人\" width=\"120\" />\r\n          <el-table-column prop=\"action\" label=\"操作\" width=\"150\" />\r\n          <el-table-column prop=\"target\" label=\"操作对象\" />\r\n          <el-table-column prop=\"status\" label=\"状态\" width=\"100\">\r\n            <template slot-scope=\"scope\">\r\n              <el-tag :type=\"scope.row.status === '成功' ? 'success' : 'warning'\" size=\"mini\">\r\n                {{ scope.row.status }}\r\n              </el-tag>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n      </div>\r\n    </div>\r\n  </Layout>\r\n</template>\r\n\r\n<script>\r\nimport Layout from '@/components/Layout.vue'\r\n\r\nexport default {\r\n  name: 'Dashboard',\r\n  components: {\r\n    Layout\r\n  },\r\n  data() {\r\n    return {\r\n      stats: {\r\n        todayRecharge: 0,\r\n        yesterdayRecharge: 0,\r\n        weekRecharge: 0,\r\n        monthRecharge: 0,\r\n        todayConsume: 0,\r\n        yesterdayConsume: 0,\r\n        weekConsume: 0,\r\n        monthConsume: 0,\r\n        todayLessons: 0,\r\n        todayStudents: 0\r\n      },\r\n      recentActivity: []\r\n    }\r\n  },\r\n  created() {\r\n    this.loadDashboardData()\r\n  },\r\n  methods: {\r\n    async loadDashboardData() {\r\n      try {\r\n        // 模拟加载数据\r\n        this.stats = {\r\n          todayRecharge: 0,\r\n          yesterdayRecharge: 8,\r\n          weekRecharge: 25,\r\n          monthRecharge: 156,\r\n          todayConsume: 0,\r\n          yesterdayConsume: 12,\r\n          weekConsume: 45,\r\n          monthConsume: 203,\r\n          todayLessons: 0,\r\n          todayStudents: 0\r\n        }\r\n        \r\n        this.recentActivity = [\r\n          {\r\n            time: '2024-01-15 14:30',\r\n            user: '张老师',\r\n            action: '添加课程',\r\n            target: '高中数学基础课程',\r\n            status: '成功'\r\n          },\r\n          {\r\n            time: '2024-01-15 13:15',\r\n            user: '李老师',\r\n            action: '学员充值',\r\n            target: '王小明 - 20课时',\r\n            status: '成功'\r\n          },\r\n          {\r\n            time: '2024-01-15 12:00',\r\n            user: '王老师',\r\n            action: '课程消课',\r\n            target: '初中英语提高班',\r\n            status: '成功'\r\n          }\r\n        ]\r\n        \r\n      } catch (error) {\r\n        console.error('加载仪表盘数据失败:', error)\r\n      }\r\n    },\r\n    \r\n    viewStats() {\r\n      this.$message.info('跳转到统计页面')\r\n    },\r\n    \r\n    addConsumption() {\r\n      this.$message.info('打开消课功能')\r\n    },\r\n    \r\n    studentRecharge() {\r\n      this.$message.info('打开充值功能')\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.dashboard {\r\n  max-width: 1400px;\r\n  margin: 0 auto;\r\n  padding: 0;\r\n}\r