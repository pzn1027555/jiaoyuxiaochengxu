{"ast":null,"code":"export default {\n  name: 'ClassFeedback',\n  data() {\n    return {\n      loading: false,\n      searchForm: {\n        courseName: '',\n        status: '',\n        type: ''\n      },\n      feedbackList: [{\n        id: 1,\n        courseName: '高二数学提高班',\n        studentName: '张同学',\n        type: 'quality',\n        rating: 5,\n        content: '老师讲解很清晰，课程内容丰富，对我帮助很大',\n        createTime: '2024-07-20 14:30:00',\n        status: 1\n      }, {\n        id: 2,\n        courseName: '初三物理竞赛班',\n        studentName: '李同学',\n        type: 'technical',\n        rating: 3,\n        content: '视频卡顿严重，影响听课效果，希望改进',\n        createTime: '2024-07-20 15:45:00',\n        status: 0\n      }, {\n        id: 3,\n        courseName: '高一英语基础班',\n        studentName: '王同学',\n        type: 'content',\n        rating: 4,\n        content: '内容很好，但是希望能增加一些练习题',\n        createTime: '2024-07-19 16:20:00',\n        status: 1\n      }, {\n        id: 4,\n        courseName: '高三化学冲刺班',\n        studentName: '赵同学',\n        type: 'quality',\n        rating: 5,\n        content: '老师非常专业，讲解透彻，非常满意',\n        createTime: '2024-07-19 10:15:00',\n        status: 1\n      }, {\n        id: 5,\n        courseName: '初二数学基础班',\n        studentName: '刘同学',\n        type: 'other',\n        rating: 2,\n        content: '上课时间太晚了，希望能调整到下午',\n        createTime: '2024-07-18 20:30:00',\n        status: 0\n      }],\n      pagination: {\n        current: 1,\n        pageSize: 10,\n        total: 5\n      }\n    };\n  },\n  computed: {\n    stats() {\n      return {\n        total: this.feedbackList.length,\n        pending: this.feedbackList.filter(item => item.status === 0).length,\n        processed: this.feedbackList.filter(item => item.status === 1).length,\n        avgRating: (this.feedbackList.reduce((sum, item) => sum + item.rating, 0) / this.feedbackList.length).toFixed(1)\n      };\n    }\n  },\n  methods: {\n    handleSearch() {\n      this.loading = true;\n      setTimeout(() => {\n        this.loading = false;\n        this.$message.success('搜索完成');\n      }, 1000);\n    },\n    handleReset() {\n      this.searchForm = {\n        courseName: '',\n        status: '',\n        type: ''\n      };\n      this.handleSearch();\n    },\n    handleCreate() {\n      this.$message.info('创建反馈功能开发中...');\n    },\n    handleView(row) {\n      this.$message.info(`查看反馈详情：${row.courseName}`);\n    },\n    handleProcess(row) {\n      this.$confirm(`确定要处理\"${row.courseName}\"的反馈吗？`, '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'success'\n      }).then(() => {\n        row.status = 1;\n        this.$message.success('反馈处理成功');\n      });\n    },\n    handleIgnore(row) {\n      this.$confirm(`确定要忽略\"${row.courseName}\"的反馈吗？`, '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        row.status = 2;\n        this.$message.success('反馈已忽略');\n      });\n    },\n    handleSizeChange(val) {\n      this.pagination.pageSize = val;\n      this.handleSearch();\n    },\n    handleCurrentChange(val) {\n      this.pagination.current = val;\n      this.handleSearch();\n    },\n    getTypeColor(type) {\n      const colorMap = {\n        quality: 'success',\n        content: 'primary',\n        technical: 'warning',\n        other: 'info'\n      };\n      return colorMap[type] || 'info';\n    },\n    getTypeText(type) {\n      const textMap = {\n        quality: '教学质量',\n        content: '课程内容',\n        technical: '技术问题',\n        other: '其他'\n      };\n      return textMap[type] || '未知';\n    },\n    getStatusType(status) {\n      const statusMap = {\n        0: 'warning',\n        1: 'success',\n        2: 'info'\n      };\n      return statusMap[status] || 'info';\n    },\n    getStatusText(status) {\n      const statusMap = {\n        0: '待处理',\n        1: '已处理',\n        2: '已忽略'\n      };\n      return statusMap[status] || '未知';\n    }\n  },\n  mou