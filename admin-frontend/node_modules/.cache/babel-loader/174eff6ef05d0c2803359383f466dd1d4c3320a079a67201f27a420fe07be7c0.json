form-item label=\"问卷标题\">\r\n              <el-input v-model=\"searchForm.title\" placeholder=\"请输入问卷标题\" clearable />\r\n            </el-form-item>\r\n            <el-form-item label=\"问卷状态\">\r\n              <el-select v-model=\"searchForm.status\" placeholder=\"请选择状态\" clearable>\r\n                <el-option label=\"草稿\" value=\"0\" />\r\n                <el-option label=\"发布中\" value=\"1\" />\r\n                <el-option label=\"已结束\" value=\"2\" />\r\n              </el-select>\r\n            </el-form-item>\r\n            <el-form-item>\r\n              <el-button type=\"primary\" @click=\"handleSearch\">查询</el-button>\r\n              <el-button @click=\"handleReset\">重置</el-button>\r\n            </el-form-item>\r\n          </el-form>\r\n        </div>\r\n\r\n        <el-table :data=\"questionnaireList\" style=\"width: 100%\" v-loading=\"loading\">\r\n          <el-table-column prop=\"id\" label=\"问卷ID\" width=\"80\" />\r\n          <el-table-column prop=\"title\" label=\"问卷标题\" min-width=\"200\" />\r\n          <el-table-column prop=\"category\" label=\"分类\" width=\"120\" />\r\n          <el-table-column prop=\"questionCount\" label=\"题目数量\" width=\"100\">\r\n            <template slot-scope=\"scope\">\r\n              {{ scope.row.questionCount }} 题\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column prop=\"responseCount\" label=\"回答数量\" width=\"100\" />\r\n          <el-table-column prop=\"createTime\" label=\"创建时间\" width=\"160\" />\r\n          <el-table-column prop=\"status\" label=\"状态\" width=\"100\">\r\n            <template slot-scope=\"scope\">\r\n              <el-tag :type=\"getStatusType(scope.row.status)\">\r\n                {{ getStatusText(scope.row.status) }}\r\n              </el-tag>\r\n            </template>\r\n          </el-table-column>\r\n          <el-table-column label=\"操作\" width=\"240\" fixed=\"right\">\r\n            <template slot-scope=\"scope\">\r\n              <el-button type=\"t