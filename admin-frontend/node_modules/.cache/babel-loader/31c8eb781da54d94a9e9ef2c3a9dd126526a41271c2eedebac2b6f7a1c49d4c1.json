{"ast":null,"code":"export default {\n  name: 'Layout',\n  computed: {\n    userInfo() {\n      return this.$store.getters.getUser || {\n        username: 'Admin',\n        nickname: '管理员'\n      };\n    },\n    sidebarCollapsed() {\n      return this.$store.getters.getSidebarCollapsed;\n    },\n    sidebarWidth() {\n      return this.sidebarCollapsed ? '64px' : '220px';\n    },\n    activeMenu() {\n      const path = this.$route.path;\n      // 处理子菜单的激活状态\n      if (path.includes('/teacher')) return '/teachers';\n      if (path.includes('/order') || path.includes('/finance')) return '/orders';\n      return path;\n    }\n  },\n  methods: {\n    toggleSidebar() {\n      this.$store.dispatch('toggleSidebar');\n    },\n    handleCommand(command) {\n      switch (command) {\n        case 'profile':\n          this.$message.info('个人设置功能开发中...');\n          break;\n        case 'logout':\n          this.handleLogout();\n          break;\n      }\n    },\n    handleLogout() {\n      this.$confirm('确定要退出登录吗？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.$store.dispatch('logout');\n        this.$message.success('退出登录成功');\n        this.$router.push('/login');\n      }).catch(() => {\n        // 取消操作\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","computed","userInfo","$store","getters","getUser","username","nickname","sidebarCollapsed","getSidebarCollapsed","sidebarWidth","activeMenu","path","$route","includes","methods","toggleSidebar","dispatch","handleCommand","command","$message","info","handleLogout","$confirm","confirmButtonText","cancelButtonText","type","then","success","$router","push","catch"],"sources":["src/components/Layout.vue"],"sourcesContent":["<template>\n  <div class=\"layout-container\">\n    <el-container>\n      <!-- 头部 -->\n      <el-header class=\"layout-header\">\n        <div class=\"header-left\">\n          <el-button \n            type=\"text\" \n            icon=\"el-icon-s-fold\" \n            @click=\"toggleSidebar\"\n            class=\"sidebar-toggle\"\n          />\n          <h3 class=\"logo\">\n            <i class=\"el-icon-reading\" />\n            管理系统平台\n          </h3>\n        </div>\n        \n        <div class=\"header-right\">\n          <div class=\"notification-area\">\n            <el-badge :value=\"3\" class=\"notification-badge\">\n              <el-button type=\"text\" icon=\"el-icon-bell\" class=\"notification-btn\" />\n            </el-badge>\n          </div>\n          <el-dropdown @command=\"handleCommand\" trigger=\"click\">\n            <span class=\"user-info\">\n              <el-avatar \n                :size=\"32\" \n                :src=\"userInfo.avatar\" \n                icon=\"el-icon-user-solid\"\n                class=\"user-avatar\"\n              />\n              <span class=\"username\">欢迎您，{{ userInfo.nickname || userInfo.username || 'George Li' }}</span>\n              <i class=\"el-icon-arrow-down\" />\n            </span>\n            <el-dropdown-menu slot=\"dropdown\">\n              <el-dropdown-item command=\"profile\">\n                <i class=\"el-icon-user\"></i> 个人设置\n              </el-dropdown-item>\n              <el-dropdown-item command=\"logout\" divided>\n                <i class=\"el-icon-switch-button\"></i> 退出登录\n              </el-dropdown-item>\n            </el-dropdown-menu>\n          </el-dropdown>\n        </div>\n      </el-header>\n      \n      <el-container>\n        <!-- 侧边栏 -->\n        <el-aside :width=\"sidebarWidth\" class=\"layout-aside\">\n          <div class=\"sidebar-content\">\n            <el-menu\n              :default-active=\"activeMenu\"\n              :collapse=\"sidebarCollapsed\"\n              :unique-opened=\"true\"\n              router\n              class=\"sidebar-menu\"\n            >\n              <!-- 数据研究 -->\n              <div class=\"menu-section\">\n                <div v-if=\"!sidebarCollapsed\" class=\"section-title\">数据研究</div>\n                <el-menu-item index=\"/dashboard\" class=\"menu-item\">\n                  <div class=\"menu-item-content\">\n                    <i class=\"el-icon-s-data menu-icon\" />\n                    <span slot=\"title\">数学教研</span>\n                  </div>\n                </el-menu-item>\n              </div>\n              \n              <!-- 人事行政 -->\n              <div class=\"menu-section\">\n                <div v-if=\"!sidebarCollapsed\" class=\"section-title\">人事行政</div>\n                <el-submenu index=\"teachers\" class=\"menu-submenu\">\n                  <template slot=\"title\">\n                    <div class=\"menu-item-content\">\n                      <i class=\"el-icon-user menu-icon\" />\n                      <span>教师管理</span>\n                    </div>\n                  </template>\n                  <el-menu-item index=\"/teachers\" class=\"submenu-item\">\n                    <span>教师列表</span>\n                  </el-menu-item>\n                  <el-menu-item index=\"/teacher-review\" class=\"submenu-item\">\n                    <span>教师审核</span>\n                  </el-menu-item>\n                </el-submenu>\n              </div>\n              \n              <!-- 薪火计划 -->\n              <div class=\"menu-section\">\n                <div v-if=\"!sidebarCollapsed\" class=\"section-title\">薪火计划</div>\n                <el-submenu index=\"students\" class=\"menu-submenu\">\n                  <template slot=\"title\">\n                    <div class=\"menu-item-content\">\n                      <i class=\"el-icon-user-solid menu-icon\" />\n                      <span>学生管理</span>\n                    </div>\n                  </template>\n                  <el-menu-item index=\"/students/one-to-one\" class=\"submenu-item\">\n                    <span>一对一课程反馈</span>\n                  </el-menu-item>\n                  <el-menu-item index=\"/students\" class=\"submenu-item\">\n                    <span>一对一学员</span>\n                  </el-menu-item>\n                </el-submenu>\n              </div>\n              \n              <!-- 考试管理 -->\n              <div class=\"menu-section\">\n                <div v-if=\"!sidebarCollapsed\" class=\"section-title\">考试管理</div>\n                <el-menu-item index=\"/exams\" class=\"menu-item\">\n                  <div class=\"menu-item-content\">\n                    <i class=\"el-icon-document menu-icon\" />\n                    <span slot=\"title\">考试管理</span>\n                  </div>\n                </el-menu-item>\n              </div>\n              \n              <!-- 竞赛课程 -->\n              <div class=\"menu-section\">\n                <div v-if=\"!sidebarCollapsed\" class=\"section-title\">竞赛课程</div>\n                <el-submenu index=\"courses\" class=\"menu-submenu\">\n                  <template slot=\"title\">\n                    <div class=\"menu-item-content\">\n                      <i class=\"el-icon-reading menu-icon\" />\n                      <span>课程管理</span>\n                    </div>\n                  </template>\n                  <el-menu-item index=\"/courses\" class=\"submenu-item\">\n                    <span>课程列表</span>\n                  </el-menu-item>\n                  <el-menu-item index=\"/course-categories\" class=\"submenu-item\">\n                    <span>课程分类</span>\n                  </el-menu-item>\n                  <el-menu-item index=\"/video-clips\" class=\"submenu-item\">\n                    <span>视频打点</span>\n                  </el-menu-item>\n                </el-submenu>\n              </div>\n              \n              <!-- 系统管理 -->\n              <div class=\"menu-section\">\n                <div v-if=\"!sidebarCollapsed\" class=\"section-title\">系统管理</div>\n                <el-submenu index=\"system\" class=\"menu-submenu\">\n                  <template slot=\"title\">\n                    <div class=\"menu-item-content\">\n                      <i class=\"el-icon-setting menu-icon\" />\n                      <span>系统管理</span>\n                    </div>\n                  </template>\n                  <el-menu-item index=\"/system/leave-request\" class=\"submenu-item\">\n                    <span>网课模考评请</span>\n                  </el-menu-item>\n                  <el-menu-item index=\"/system/questionnaire\" class=\"submenu-item\">\n                    <span>问卷调查管理</span>\n                  </el-menu-item>\n                  <el-menu-item index=\"/system/statistics\" class=\"submenu-item\">\n                    <span>教师数据统计</span>\n                  </el-menu-item>\n                </el-submenu>\n              </div>\n            </el-menu>\n          </div>\n        </el-aside>\n        \n        <!-- 主内容区 -->\n        <el-main class=\"layout-main\">\n          <div class=\"main-content\">\n            <slot />\n          </div>\n        </el-main>\n      </el-container>\n    </el-container>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'Layout',\n  computed: {\n    userInfo() {\n      return this.$store.getters.getUser || { username: 'Admin', nickname: '管理员' }\n    },\n    sidebarCollapsed() {\n      return this.$store.getters.getSidebarCollapsed\n    },\n    sidebarWidth() {\n      return this.sidebarCollapsed ? '64px' : '220px'\n    },\n    activeMenu() {\n      const path = this.$route.path\n      // 处理子菜单的激活状态\n      if (path.includes('/teacher')) return '/teachers'\n      if (path.includes('/order') || path.includes('/finance')) return '/orders'\n      return path\n    }\n  },\n  methods: {\n    toggleSidebar() {\n      this.$store.dispatch('toggleSidebar')\n    },\n    handleCommand(command) {\n      switch (command) {\n        case 'profile':\n          this.$message.info('个人设置功能开发中...')\n          break\n        case 'logout':\n          this.handleLogout()\n          break\n      }\n    },\n    handleLogout() {\n      this.$confirm('确定要退出登录吗？', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.$store.dispatch('logout')\n        this.$message.success('退出登录成功')\n        this.$router.push('/login')\n      }).catch(() => {\n        // 取消操作\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.layout-container {\n  height: 100vh;\n}\n\n.layout-header {\n  background: #ffffff;\n  border-bottom: 1px solid #e8e8e8;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0 24px;\n  box-shadow: 0 1px 4px rgba(0, 21, 41, 0.08);\n  height: 60px;\n}\n\n.header-left {\n  display: flex;\n  align-items: center;\n}\n\n.sidebar-toggle {\n  font-size: 18px;\n  color: #606266;\n  margin-right: 16px;\n  padding: 8px;\n  border-radius: 4px;\n  transition: all 0.3s;\n}\n\n.sidebar-toggle:hover {\n  color: #409eff;\n  background: #ecf5ff;\n}\n\n.logo {\n  color: #303133;\n  margin: 0;\n  font-size: 18px;\n  font-weight: 600;\n  display: flex;\n  align-items: center;\n  gap: 8px;\n}\n\n.logo i {\n  font-size: 20px;\n  color: #409eff;\n}\n\n.header-right {\n  display: flex;\n  align-items: center;\n}\n\n.notification-area {\n  margin-right: 16px;\n}\n\n.notification-btn {\n  color: #606266;\n  font-size: 18px;\n  padding: 8px;\n  border-radius: 4px;\n  transition: all 0.3s;\n}\n\n.notification-btn:hover {\n  color: #409eff;\n  background: #ecf5ff;\n}\n\n.notification-badge {\n  margin-right: 8px;\n}\n\n.user-info {\n  display: flex;\n  align-items: center;\n  cursor: pointer;\n  padding: 6px 12px;\n  border-radius: 6px;\n  transition: background-color 0.3s;\n  color: #606266;\n}\n\n.user-info:hover {\n  background-color: #f5f7fa;\n}\n\n.user-avatar {\n  border: 2px solid #e4e7ed;\n}\n\n.username {\n  margin: 0 8px;\n  font-size: 14px;\n  font-weight: 500;\n}\n\n.layout-aside {\n  background: #ffffff;\n  transition: width 0.3s;\n  box-shadow: 1px 0 4px rgba(0, 21, 41, 0.08);\n  border-right: 1px solid #e8e8e8;\n}\n\n.sidebar-content {\n  height: 100%;\n  overflow-y: auto;\n  padding: 16px 0;\n}\n\n.menu-section {\n  margin-bottom: 8px;\n}\n\n.section-title {\n  padding: 8px 24px 4px;\n  font-size: 12px;\n  color: #909399;\n  font-weight: 500;\n  text-transform: uppercase;\n  letter-spacing: 0.5px;\n}\n\n.sidebar-menu {\n  border: none;\n  background: transparent;\n}\n\n.menu-item {\n  margin: 2px 16px;\n  border-radius: 6px;\n  transition: all 0.3s;\n}\n\n.menu-item:hover {\n  background: #f0f9ff !important;\n}\n\n.menu-item.is-active {\n  background: #e6f7ff !important;\n  border-right: 3px solid #409eff;\n}\n\n.menu-submenu {\n  margin: 2px 16px;\n}\n\n.menu-item-content {\n  display: flex;\n  align-items: center;\n  padding: 0 8px;\n}\n\n.menu-icon {\n  font-size: 16px;\n  color: #606266;\n  margin-right: 12px;\n  width: 16px;\n  text-align: center;\n}\n\n.submenu-item {\n  padding-left: 48px !important;\n  margin: 2px 0;\n  border-radius: 4px;\n  color: #606266;\n}\n\n.submenu-item:hover {\n  background: #f0f9ff !important;\n  color: #409eff !important;\n}\n\n.submenu-item.is-active {\n  background: #e6f7ff !important;\n  color: #409eff !important;\n  font-weight: 500;\n}\n\n.layout-main {\n  background: #f5f7fa;\n  padding: 20px;\n}\n\n.main-content {\n  min-height: calc(100vh - 120px);\n}\n\n/* Element UI 菜单样式调整 */\n.el-menu {\n  border: none;\n}\n\n.el-menu-item {\n  color: #606266;\n  font-size: 14px;\n  line-height: 40px;\n  height: 40px;\n  border: none;\n}\n\n.el-menu-item:hover {\n  background-color: #f0f9ff !important;\n  color: #409eff !important;\n}\n\n.el-menu-item.is-active {\n  background-color: #e6f7ff !important;\n  color: #409eff !important;\n  font-weight: 500;\n}\n\n.el-submenu__title {\n  color: #606266;\n  font-size: 14px;\n  line-height: 40px;\n  height: 40px;\n  border: none;\n}\n\n.el-submenu__title:hover {\n  background-color: #f0f9ff !important;\n  color: #409eff !important;\n}\n\n.el-submenu .el-menu-item {\n  background: transparent;\n  min-height: 36px;\n  line-height: 36px;\n  height: 36px;\n}\n\n.el-submenu .el-menu-item:hover {\n  background-color: #f0f9ff !important;\n  color: #409eff !important;\n}\n\n.el-submenu .el-menu-item.is-active {\n  background-color: #e6f7ff !important;\n  color: #409eff !important;\n  font-weight: 500;\n}\n\n/* 教育主题色彩 */\n.education-primary {\n  color: #2c5aa0;\n}\n\n.education-secondary {\n  color: #ffd04b;\n}\n\n.education-success {\n  color: #67c23a;\n}\n\n.education-warning {\n  color: #e6a23c;\n}\n\n.education-danger {\n  color: #f56c6c;\n}\n\n/* 响应式 */\n@media (max-width: 768px) {\n  .layout-header {\n    padding: 0 15px;\n  }\n  \n  .logo {\n    font-size: 18px;\n  }\n  \n  .logo i {\n    font-size: 20px;\n  }\n  \n  .layout-main {\n    padding: 15px;\n  }\n}\n</style> "],"mappings":"AAiLA;EACAA,IAAA;EACAC,QAAA;IACAC,SAAA;MACA,YAAAC,MAAA,CAAAC,OAAA,CAAAC,OAAA;QAAAC,QAAA;QAAAC,QAAA;MAAA;IACA;IACAC,iBAAA;MACA,YAAAL,MAAA,CAAAC,OAAA,CAAAK,mBAAA;IACA;IACAC,aAAA;MACA,YAAAF,gBAAA;IACA;IACAG,WAAA;MACA,MAAAC,IAAA,QAAAC,MAAA,CAAAD,IAAA;MACA;MACA,IAAAA,IAAA,CAAAE,QAAA;MACA,IAAAF,IAAA,CAAAE,QAAA,cAAAF,IAAA,CAAAE,QAAA;MACA,OAAAF,IAAA;IACA;EACA;EACAG,OAAA;IACAC,cAAA;MACA,KAAAb,MAAA,CAAAc,QAAA;IACA;IACAC,cAAAC,OAAA;MACA,QAAAA,OAAA;QACA;UACA,KAAAC,QAAA,CAAAC,IAAA;UACA;QACA;UACA,KAAAC,YAAA;UACA;MACA;IACA;IACAA,aAAA;MACA,KAAAC,QAAA;QACAC,iBAAA;QACAC,gBAAA;QACAC,IAAA;MACA,GAAAC,IAAA;QACA,KAAAxB,MAAA,CAAAc,QAAA;QACA,KAAAG,QAAA,CAAAQ,OAAA;QACA,KAAAC,OAAA,CAAAC,IAAA;MACA,GAAAC,KAAA;QACA;MAAA,CACA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}