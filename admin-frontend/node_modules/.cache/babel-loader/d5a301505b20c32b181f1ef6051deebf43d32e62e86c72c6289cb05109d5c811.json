{"ast":null,"code":"import TabsContainer from '@/components/TabsContainer.vue';\nexport default {\n  name: 'CourseFeedback',\n  components: {\n    TabsContainer\n  },\n  data() {\n    return {\n      loading: false,\n      activeTab: 'unfilled',\n      tabs: [{\n        key: 'unfilled',\n        label: '未填写课程反馈'\n      }, {\n        key: 'filled',\n        label: '已填写课程反馈'\n      }],\n      searchForm: {\n        courseName: '',\n        subject: '',\n        lessonName: '',\n        teacher: ''\n      },\n      unfilledList: [\n        // 模拟数据 - 未填写反馈\n      ],\n      filledList: [\n        // 模拟数据 - 已填写反馈\n      ]\n    };\n  },\n  created() {\n    this.loadData();\n  },\n  methods: {\n    handleTabChange(tab) {\n      this.activeTab = tab.key;\n      this.loadData();\n    },\n    handleSearch() {\n      this.loadData();\n    },\n    async loadData() {\n      this.loading = true;\n      try {\n        // 模拟API调用\n        await new Promise(resolve => setTimeout(resolve, 500));\n        if (this.activeTab === 'unfilled') {\n          this.unfilledList = [];\n        } else {\n          this.filledList = [];\n        }\n      } catch (error) {\n        this.$message.error('加载数据失败');\n      } finally {\n        this.loading = false;\n      }\n    },\n    getStatusText(status) {\n      const statusMap = {\n        'completed': '已完成',\n        'reviewed': '已审核',\n        'pending': '待处理'\n      };\n      return statusMap[status] || '未知';\n    },\n    getStatusClass(status) {\n      const classMap = {\n        'completed': 'active',\n        'reviewed': 'active',\n        'pending': 'pending'\n      };\n      return classMap[status] || 'default';\n    }\n  }\n};","map":{"version":3,"names":["TabsContainer","name","components","data","loading","activeTab","tabs","key","label","searchForm","courseName","subject","lessonName","teacher","unfilledList","filledList","created","loadData","methods","handleTabChange","tab","handleSearch","Promise","resolve","setTimeout","error","$message","getStatusText","status","statusMap","getStatusClass","classMap"],"sources":["src/views/education/CourseFeedback.vue"],"sourcesContent":["<template>\r\n  <div class=\"course-feedback-container\">\r\n    <!-- 页面头部 -->\r\n    <div class=\"page-header\">\r\n      <div class=\"header-content\">\r\n        <h1 class=\"page-title\">\r\n          <i class=\"el-icon-chat-dot-round\"></i>\r\n          我的课程\r\n        </h1>\r\n        <p class=\"page-description\">管理和查看课程反馈信息</p>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- 标签页容器 -->\r\n    <TabsContainer \r\n      :tabs=\"tabs\" \r\n      v-model=\"activeTab\"\r\n      @tab-change=\"handleTabChange\"\r\n    >\r\n      <template v-slot=\"{ activeTab }\">\r\n        <!-- 未填写课程反馈 -->\r\n        <div v-if=\"activeTab === 'unfilled'\" class=\"tab-panel\">\r\n          <div class=\"feedback-section\">\r\n            <div class=\"section-header\">\r\n              <h3>未填写课程反馈</h3>\r\n            </div>\r\n            \r\n            <!-- 搜索栏 -->\r\n            <div class=\"search-container\">\r\n              <el-form :model=\"searchForm\" class=\"search-form\" inline>\r\n                <el-form-item label=\"课程名\">\r\n                  <el-input\r\n                    v-model=\"searchForm.courseName\"\r\n                    placeholder=\"请输入课程名\"\r\n                    clearable\r\n                    style=\"width: 180px\"\r\n                  />\r\n                </el-form-item>\r\n                <el-form-item label=\"科目\">\r\n                  <el-input\r\n                    v-model=\"searchForm.subject\"\r\n                    placeholder=\"请输入科目\"\r\n                    clearable\r\n                    style=\"width: 120px\"\r\n                  />\r\n                </el-form-item>\r\n                <el-form-item label=\"课名\">\r\n                  <el-input\r\n                    v-model=\"searchForm.lessonName\"\r\n                    placeholder=\"请输入课名\"\r\n                    clearable\r\n                    style=\"width: 150px\"\r\n                  />\r\n                </el-form-item>\r\n                <el-form-item label=\"讲师\">\r\n                  <el-input\r\n                    v-model=\"searchForm.teacher\"\r\n                    placeholder=\"请输入讲师名\"\r\n                    clearable\r\n                    style=\"width: 150px\"\r\n                  />\r\n                </el-form-item>\r\n                <el-form-item>\r\n                  <el-button type=\"primary\" icon=\"el-icon-search\" @click=\"handleSearch\">\r\n                    查询\r\n                  </el-button>\r\n                </el-form-item>\r\n              </el-form>\r\n            </div>\r\n\r\n            <!-- 表格 -->\r\n            <div class=\"table-container\">\r\n              <el-table :data=\"unfilledList\" v-loading=\"loading\" style=\"width: 100%\">\r\n                <el-table-column prop=\"courseName\" label=\"课程名\" width=\"180\" />\r\n                <el-table-column prop=\"subject\" label=\"科目\" width=\"120\" />\r\n                <el-table-column prop=\"lessonName\" label=\"课名\" width=\"180\" />\r\n                <el-table-column prop=\"classDate\" label=\"上课日期\" width=\"120\" />\r\n                <el-table-column prop=\"classTime\" label=\"上课时间\" width=\"120\" />\r\n                <el-table-column prop=\"feedback\" label=\"课程反馈\" width=\"180\">\r\n                  <template slot-scope=\"scope\">\r\n                    <span class=\"feedback-placeholder\">{{ scope.row.feedback || '待填写' }}</span>\r\n                  </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"teacher\" label=\"讲师\" width=\"120\" />\r\n                <el-table-column prop=\"duration\" label=\"计时\" width=\"100\" />\r\n                <el-table-column prop=\"officialFeedback\" label=\"官方已反馈\" width=\"140\">\r\n                  <template slot-scope=\"scope\">\r\n                    <span :class=\"['status-tag', scope.row.officialFeedback ? 'active' : 'pending']\">\r\n                      {{ scope.row.officialFeedback ? '是' : '否' }}\r\n                    </span>\r\n                  </template>\r\n                </el-table-column>\r\n              </el-table>\r\n              \r\n              <div class=\"pagination-wrapper\">\r\n                <div class=\"pagination-info\">\r\n                  显示 1 - {{ unfilledList.length }} 条，共 {{ unfilledList.length }} 条\r\n                </div>\r\n                <el-pagination\r\n                  background\r\n                  layout=\"prev, pager, next\"\r\n                  :total=\"unfilledList.length\"\r\n                  :page-size=\"10\"\r\n                />\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- 已填写课程反馈 -->\r\n        <div v-if=\"activeTab === 'filled'\" class=\"tab-panel\">\r\n          <div class=\"feedback-section\">\r\n            <div class=\"section-header\">\r\n              <h3>已填写课程反馈</h3>\r\n            </div>\r\n            \r\n            <!-- 搜索栏 -->\r\n            <div class=\"search-container\">\r\n              <el-form :model=\"searchForm\" class=\"search-form\" inline>\r\n                <el-form-item label=\"课程名\">\r\n                  <el-input\r\n                    v-model=\"searchForm.courseName\"\r\n                    placeholder=\"请输入课程名\"\r\n                    clearable\r\n                    style=\"width: 180px\"\r\n                  />\r\n                </el-form-item>\r\n                <el-form-item label=\"科目\">\r\n                  <el-input\r\n                    v-model=\"searchForm.subject\"\r\n                    placeholder=\"请输入科目\"\r\n                    clearable\r\n                    style=\"width: 120px\"\r\n                  />\r\n                </el-form-item>\r\n                <el-form-item label=\"课名\">\r\n                  <el-input\r\n                    v-model=\"searchForm.lessonName\"\r\n                    placeholder=\"请输入课名\"\r\n       