{"ast":null,"code":"import Layout from '@/components/Layout.vue';\nexport default {\n  name: 'Content',\n  components: {\n    Layout\n  },\n  data() {\n    return {\n      statistics: {\n        banners: 5,\n        articles: 128,\n        notices: 12,\n        faq: 45\n      },\n      recentUpdates: [{\n        type: '轮播图',\n        title: '新年活动宣传图',\n        updateTime: '2024-01-16 14:30:00',\n        status: '已发布'\n      }, {\n        type: '文章',\n        title: '如何选择适合的编程课程',\n        updateTime: '2024-01-16 13:45:00',\n        status: '草稿'\n      }, {\n        type: '公告',\n        title: '系统维护通知',\n        updateTime: '2024-01-16 12:20:00',\n        status: '已发布'\n      }, {\n        type: 'FAQ',\n        title: '如何申请退款？',\n        updateTime: '2024-01-16 11:15:00',\n        status: '已发布'\n      }]\n    };\n  },\n  methods: {\n    handleCreate() {\n      this.$message.info('新建内容功能开发中...');\n    },\n    handleBanners() {\n      this.$message.info('轮播图管理功能开发中...');\n    },\n    handleArticles() {\n      this.$message.info('文章管理功能开发中...');\n    },\n    handleNotices() {\n      this.$message.info('公告管理功能开发中...');\n    },\n    handleFaq() {\n      this.$message.info('FAQ管理功能开发中...');\n    },\n    handleBatchUpload() {\n      this.$message.info('批量上传功能开发中...');\n    },\n    handleContentAudit() {\n      this.$message.info('内容审核功能开发中...');\n    },\n    handleContentStats() {\n      this.$message.info('内容统计功能开发中...');\n    },\n    handleContentBackup() {\n      this.$message.info('内容备份功能开发中...');\n    },\n    editContent(content) {\n      this.$message.info(`编辑${content.type}: ${content.title}`);\n    },\n    viewContent(content) {\n      this.$message.info(`查看${content.type}: ${content.title}`);\n    },\n    getTypeColor(type) {\n      const colorMap = {\n        '轮播图': 'primary',\n        '文章': 'success',\n        '公告': 'warning',\n        'FAQ': 'info'\n      };\n      return colorMap[type] || 'default';\n    }\n  }\n};","map":{"version":3,"names":["Layout","name","components","data","statistics","banners","articles","notices","faq","recentUpdates","type","title","updateTime","status","methods","handleCreate","$message","info","handleBanners","handleArticles","handleNotices","handleFaq","handleBatchUpload","handleContentAudit","handleContentStats","handleContentBackup","editContent","content","viewContent","getTypeColor","colorMap"],"sources":["src/views/content/Content.vue"],"sourcesContent":["<template>\r\n  <Layout>\r\n    <div class=\"content-container\">\r\n      <!-- 页面头部 -->\r\n      <div class=\"page-header\">\r\n        <div class=\"header-content\">\r\n          <h1 class=\"page-title\">\r\n            <i class=\"el-icon-document\"></i>\r\n            内容管理\r\n          </h1>\r\n          <p class=\"page-description\">管理平台各类内容信息</p>\r\n        </div>\r\n        <div class=\"header-actions\">\r\n          <el-button type=\"primary\" icon=\"el-icon-plus\" @click=\"handleCreate\">\r\n            新建内容\r\n          </el-button>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 内容分类卡片 -->\r\n      <div class=\"content-cards\">\r\n        <div class=\"content-card\" @click=\"handleBanners\">\r\n          <div class=\"card-icon\">\r\n            <i class=\"el-icon-picture\"></i>\r\n          </div>\r\n          <div class=\"card-content\">\r\n            <h3>轮播图管理</h3>\r\n            <p>管理首页轮播图片</p>\r\n            <div class=\"card-count\">{{ statistics.banners || 0 }} 条</div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"content-card\" @click=\"handleArticles\">\r\n          <div class=\"card-icon\">\r\n            <i class=\"el-icon-edit\"></i>\r\n          </div>\r\n          <div class=\"card-content\">\r\n            <h3>文章管理</h3>\r\n            <p>管理平台文章资讯</p>\r\n            <div class=\"card-count\">{{ statistics.articles || 0 }} 篇</div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"content-card\" @click=\"handleNotices\">\r\n          <div class=\"card-icon\">\r\n            <i class=\"el-icon-bell\"></i>\r\n          </div>\r\n          <div class=\"card-content\">\r\n            <h3>公告管理</h3>\r\n            <p>管理系统公告通知</p>\r\n            <div class=\"card-count\">{{ statistics.notices || 0 }} 条</div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"content-card\" @click=\"handleFaq\">\r\n          <div class=\"card-icon\">\r\n            <i class=\"el-icon-question\"></i>\r\n          </div>\r\n          <div class=\"card-content\">\r\n            <h3>FAQ管理</h3>\r\n            <p>管理常见问题</p>\r\n            <div class=\"card-count\">{{ statistics.faq || 0 }} 条</div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 内容管理工具 -->\r\n      <div class=\"content-tools\">\r\n        <el-card>\r\n          <div slot=\"header\">\r\n            <span>内容工具</span>\r\n          </div>\r\n          <el-row :gutter=\"20\">\r\n            <el-col :span=\"6\">\r\n              <div class=\"tool-item\" @click=\"handleBatchUpload\">\r\n                <i class=\"el-icon-upload\"></i>\r\n                <span>批量上传</span>\r\n              </div>\r\n            </el-col>\r\n            <el-col :span=\"6\">\r\n              <div class=\"tool-item\" @click=\"handleContentAudit\">\r\n                <i class=\"el-icon-s-check\"></i>\r\n                <span>内容审核</span>\r\n              </div>\r\n            </el-col>\r\n            <el-col :span=\"6\">\r\n              <div class=\"tool-item\" @click=\"handleContentStats\">\r\n                <i class=\"el-icon-s-data\"></i>\r\n                <span>内容统计</span>\r\n              </div>\r\n            </el-col>\r\n            <el-col :span=\"6\">\r\n              <div class=\"tool-item\" @click=\"handleContentBackup\">\r\n                <i class=\"el-icon-download\"></i>\r\n                <span>内容备份</span>\r\n              </div>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </div>\r\n\r\n      <!-- 最近更新 -->\r\n      <div class=\"recent-updates\">\r\n        <el-card>\r\n          <div slot=\"header\">\r\n            <span>最近更新</span>\r\n          </div>\r\n          <el-table :data=\"recentUpdates\" size=\"small\">\r\n            <el-table-column prop=\"type\" label=\"类型\" width=\"100\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag size=\"mini\" :type=\"getTypeColor(scope.row.type)\">\r\n                  {{ scope.row.type }}\r\n                </el-tag>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column prop=\"title\" label=\"标题\" />\r\n            <el-table-column prop=\"updateTime\" label=\"更新时间\" width=\"180\" />\r\n            <el-table-column prop=\"status\" label=\"状态\" width=\"100\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag size=\"mini\" :type=\"scope.row.status === '已发布' ? 'success' : 'warning'\">\r\n                  {{ scope.row.status }}\r\n                </el-tag>\r\n              </template>\r\n            </el-table-column>\r\n            <el-table-column label=\"操作\" width=\"150\">\r\n              <template slot-scope=\"scope\">\r\n                <el-button size=\"mini\" type=\"text\" @click=\"editContent(scope.row)\">\r\n                  编辑\r\n                </el-button>\r\n                <el-button size=\"mini\" type=\"text\" @click=\"viewContent(scope.row)\">\r\n                  查看\r\n                </el-button>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n        </el-card>\r\n      </div>\r\n    </div>\r\n  </Layout>\r\n</template>\r\n\r\n<script>\r\nimport Layout from '@/components/Layout.vue'\r\n\r\nexport default {\r\n  name: 'Content',\r\n  components: {\r\n    Layout\r\n  },\r\n  data() {\r\n    return {\r\n      statistics: {\r\n        banners: 5,\r\n        articles: 128,\r\n        notices: 12,\r\n        faq: 45\r\n      },\r\n      recentUpdates: [\r\n        {\r\n          type: '轮播图',\r\n          title: '新年活动宣传图',\r\n          updateTime: '2024-01-16 14:30:00',\r\n          status: '已发布'\r\n        },\r\n        {\r\n          type: '文章',\r\n          title: '如何选择适合的编程课程',\r\n          updateTime: '2024-01-16 13:45:00',\r\n          status: '草稿'\r\n        },\r\n        {\r\n          type: '公告',\r\n          title: '系统维护通知',\r\n          updateTime: '2024-01-16 12:20:00',\r\n          status: '已发布'\r\n        },\r\n        {\r\n          type: 'FAQ',\r\n          title: '如何申请退款？',\r\n          updateTime: '2024-01-16 11:15:00',\r\n          status: '已发布'\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  methods: {\r\n    handleCreate() {\r\n      this.$message.info('新建内容功能开发中...')\r\n    },\r\n    \r\n    handleBanners() {\r\n      this.$message.info('轮播图管理功能开发中...')\r\n    },\r\n    \r\n    handleArticles() {\r\n      this.$message.info('文章管理功能开发中...')\r\n    },\r\n    \r\n    handleNotices() {\r\n      this.$message.info('公告管理功能开发中...')\r\n    },\r\n    \r\n    handleFaq() {\r\n      this.$message.info('FAQ管理功能开发中...')\r\n    },\r\n    \r\n    handleBatchUpload() {\r\n      this.$message.info('批量上传功能开发中...')\r\n    },\r\n    \r\n    handleContentAudit() {\r\n      this.$message.info('内容审核功能开发中...')\r\n    },\r\n    \r\n    handleContentStats() {\r\n      this.$message.info('内容统计功能开发中...')\r\n    },\r\n    \r\n    handleContentBackup() {\r\n      this.$message.info('内容备份功能开发中...')\r\n    },\r\n    \r\n    editContent(content) {\r\n      this.$message.info(`编辑${content.type}: ${content.title}`)\r\n    },\r\n    \r\n    viewContent(content) {\r\n      this.$message.info(`查看${content.type}: ${content.title}`)\r\n    },\r\n    \r\n    getTypeColor(type) {\r\n      const colorMap = {\r\n        '轮播图': 'primary',\r\n        '文章': 'success',\r\n        '公告': 'warning',\r\n        'FAQ': 'info'\r\n      }\r\n      return colorMap[type] || 'default'\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.content-container {\r\n  padding: 20px;\r\n  background-color: #f5f7fa;\r\n  min-height: 100vh;\r\n}\r\n\r\n.page-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n  padding: 20px;\r\n  background: linear-gradient(135deg, #2c5aa0 0%, #1e3d72 100%);\r\n  border-radius: 10px;\r\n  color: white;\r\n}\r\n\r\n.header-content h1 {\r\n  margin: 0 0 5px 0;\r\n  font-size: 24px;\r\n  font-weight: 600;\r\n}\r\n\r\n.header-content p {\r\n  margin: 0;\r\n  opacity: 0.9;\r\n  font-size: 14px;\r\n}\r\n\r\n.page-title i {\r\n  margin-right: 8px;\r\n  color: #ffd04b;\r\n}\r\n\r\n.content-cards {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\r\n  gap: 20px;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.content-card {\r\n  background: white;\r\n  border-radius: 10px;\r\n  padding: 24px;\r\n  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.1);\r\n  cursor: pointer;\r\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.content-card:hover {\r\n  transform: translateY(-5px);\r\n  box-shadow: 0 4px 20px rgba(0, 0, 0, 0.15);\r\n}\r\n\r\n.card-icon {\r\n  width: 60px;\r\n  height: 60px;\r\n  border-radius: 10px;\r\n  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  margin-right: 16px;\r\n}\r\n\r\n.card-icon i {\r\n  font-size: 24px;\r\n  color: white;\r\n}\r\n\r\n.card-content {\r\n  flex: 1;\r\n}\r\n\r\n.card-content h3 {\r\n  margin: 0 0 8px 0;\r\n  font-size: 16px;\r\n  color: #2c3e50;\r\n}\r\n\r\n.card-content p {\r\n  margin: 0 0 8px 0;\r\n  font-size: 12px;\r\n  color: #7f8c8d;\r\n}\r\n\r\n.card-count {\r\n  font-size: 14px;\r\n  font-weight: 600;\r\n  color: #667eea;\r\n}\r\n\r\n.content-tools {\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.content-tools .el-card {\r\n  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.tool-item {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.tool-item:hover {\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.tool-item i {\r\n  font-size: 24px;\r\n  color: #667eea;\r\n  margin-bottom: 8px;\r\n}\r\n\r\n.tool-item span {\r\n  font-size: 14px;\r\n  color: #2c3e50;\r\n}\r\n\r\n.recent-updates .el-card {\r\n  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.1);\r\n}\r\n</style> "],"mappings":"AA8IA,OAAAA,MAAA;AAEA;EACAC,IAAA;EACAC,UAAA;IACAF;EACA;EACAG,KAAA;IACA;MACAC,UAAA;QACAC,OAAA;QACAC,QAAA;QACAC,OAAA;QACAC,GAAA;MACA;MACAC,aAAA,GACA;QACAC,IAAA;QACAC,KAAA;QACAC,UAAA;QACAC,MAAA;MACA,GACA;QACAH,IAAA;QACAC,KAAA;QACAC,UAAA;QACAC,MAAA;MACA,GACA;QACAH,IAAA;QACAC,KAAA;QACAC,UAAA;QACAC,MAAA;MACA,GACA;QACAH,IAAA;QACAC,KAAA;QACAC,UAAA;QACAC,MAAA;MACA;IAEA;EACA;EACAC,OAAA;IACAC,aAAA;MACA,KAAAC,QAAA,CAAAC,IAAA;IACA;IAEAC,cAAA;MACA,KAAAF,QAAA,CAAAC,IAAA;IACA;IAEAE,eAAA;MACA,KAAAH,QAAA,CAAAC,IAAA;IACA;IAEAG,cAAA;MACA,KAAAJ,QAAA,CAAAC,IAAA;IACA;IAEAI,UAAA;MACA,KAAAL,QAAA,CAAAC,IAAA;IACA;IAEAK,kBAAA;MACA,KAAAN,QAAA,CAAAC,IAAA;IACA;IAEAM,mBAAA;MACA,KAAAP,QAAA,CAAAC,IAAA;IACA;IAEAO,mBAAA;MACA,KAAAR,QAAA,CAAAC,IAAA;IACA;IAEAQ,oBAAA;MACA,KAAAT,QAAA,CAAAC,IAAA;IACA;IAEAS,YAAAC,OAAA;MACA,KAAAX,QAAA,CAAAC,IAAA,MAAAU,OAAA,CAAAjB,IAAA,KAAAiB,OAAA,CAAAhB,KAAA;IACA;IAEAiB,YAAAD,OAAA;MACA,KAAAX,QAAA,CAAAC,IAAA,MAAAU,OAAA,CAAAjB,IAAA,KAAAiB,OAAA,CAAAhB,KAAA;IACA;IAEAkB,aAAAnB,IAAA;MACA,MAAAoB,QAAA;QACA;QACA;QACA;QACA;MACA;MACA,OAAAA,QAAA,CAAApB,IAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}