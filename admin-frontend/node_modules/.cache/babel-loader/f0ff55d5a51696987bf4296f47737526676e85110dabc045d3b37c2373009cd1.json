sage.success('结算处理成功');\n      }).catch(() => {});\n    }\n  }\n};","map":{"version":3,"names":["Layout","name","components","data","financeData","totalRevenue","totalCommission","netProfit","pendingSettlement","recentTransactions","orderNo","amount","type","time","pendingSettlements","teacherName","status","id","methods","exportFinanceData","$message","info","refreshData","success","handleSettlement","handleRefund","handleInvoice","handleTax","viewAllTransactions","viewAllSettlements","processSettlement","settlement","$confirm","confirmButtonText","cancelButtonText","then","catch"],"sources":["src/views/finance/Finance.vue"],"sourcesContent":["<template>\r\n  <Layout>\r\n    <div class=\"finance-container\">\r\n      <!-- 页面头部 -->\r\n      <div class=\"page-header\">\r\n        <div class=\"header-content\">\r\n          <h1 class=\"page-title\">\r\n            <i class=\"el-icon-s-finance\"></i>\r\n            财务管理\r\n          </h1>\r\n          <p class=\"page-description\">管理平台财务数据、结算和统计分析</p>\r\n        </div>\r\n        <div class=\"header-actions\">\r\n          <el-button type=\"primary\" icon=\"el-icon-download\" @click=\"exportFinanceData\">\r\n            导出报表\r\n          </el-button>\r\n          <el-button type=\"success\" icon=\"el-icon-refresh\" @click=\"refreshData\">\r\n            刷新数据\r\n          </el-button>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 财务统计卡片 -->\r\n      <div class=\"finance-stats\">\r\n        <div class=\"stat-card revenue\">\r\n          <div class=\"stat-icon\">\r\n            <i class=\"el-icon-coin\"></i>\r\n          </div>\r\n          <div class=\"stat-content\">\r\n            <div class=\"stat-value\">¥{{ financeData.totalRevenue }}</div>\r\n            <div class=\"stat-label\">总收入</div>\r\n            <div class=\"stat-trend\">\r\n              <i class=\"el-icon-top\"></i>\r\n              <span>环比 +15.6%</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"stat-card commission\">\r\n          <div class=\"stat-icon\">\r\n            <i class=\"el-icon-wallet\"></i>\r\n          </div>\r\n          <div class=\"stat-content\">\r\n            <div class=\"stat-value\">¥{{ financeData.totalCommission }}</div>\r\n            <div class=\"stat-label\">总佣金</div>\r\n            <div class=\"stat-trend\">\r\n              <i class=\"el-icon-top\"></i>\r\n              <span>环比 +8.3%</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"stat-card profit\">\r\n          <div class=\"stat-icon\">\r\n            <i class=\"el-icon-trophy\"></i>\r\n          </div>\r\n          <div class=\"stat-content\">\r\n            <div class=\"stat-value\">¥{{ financeData.netProfit }}</div>\r\n            <div class=\"stat-label\">净利润</div>\r\n            <div class=\"stat-trend\">\r\n              <i class=\"el-icon-top\"></i>\r\n              <span>环比 +12.1%</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <div class=\"stat-card pending\">\r\n          <div class=\"stat-icon\">\r\n            <i class=\"el-icon-time\"></i>\r\n          </div>\r\n          <div class=\"stat-content\">\r\n            <div class=\"stat-value\">¥{{ financeData.pendingSettlement }}</div>\r\n            <div class=\"stat-label\">待结算</div>\r\n            <div class=\"stat-trend\">\r\n              <i class=\"el-icon-bottom\"></i>\r\n              <span>环比 -5.2%</span>\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- 财务操作 -->\r\n      <div class=\"finance-operations\">\r\n        <el-card class=\"operation-card\">\r\n          <div slot=\"header\">\r\n            <span>快捷操作</span>\r\n          </div>\r\n          <el-row :gutter=\"20\">\r\n            <el-col :span=\"6\">\r\n              <div class=\"operation-item\" @click=\"handleSettlement\">\r\n                <div class=\"operation-icon\">\r\n                  <i class=\"el-icon-s-finance\"></i>\r\n                </div>\r\n                <div class=\"operation-text\">\r\n                  <h4>佣金结算</h4>\r\n                  <p>处理教师佣金结算</p>\r\n                </div>\r\n              </div>\r\n            </el-col>\r\n            <el-col :span=\"6\">\r\n              <div class=\"operation-item\" @click=\"handleRefund\">\r\n                <div class=\"operation-icon\">\r\n                  <i class=\"el-icon-refresh-left\"></i>\r\n                </div>\r\n                <div class=\"operation-text\">\r\n                  <h4>退款处理</h4>\r\n                  <p>处理用户退款申请</p>\r\n                </div>\r\n              </div>\r\n            </el-col>\r\n            <el-col :span=\"6\">\r\n              <div class=\"operation-item\" @click=\"handleInvoice\">\r\n                <div class=\"operation-icon\">\r\n                  <i class=\"el-icon-document\"></i>\r\n                </div>\r\n                <div class=\"operation-text\">\r\n                  <h4>发票管理</h4>\r\n                  <p>管理发票开具</p>\r\n                </div>\r\n              </div>\r\n            </el-col>\r\n            <el-col :span=\"6\">\r\n              <div class=\"operation-item\" @click=\"handleTax\">\r\n                <div class=\"operation-icon\">\r\n                  <i class=\"el-icon-s-data\"></i>\r\n                </div>\r\n                <div class=\"operation-text\">\r\n                  <h4>税务管理</h4>\r\n                  <p>税务申报和管理</p>\r\n                </div>\r\n              </div>\r\n            </el-col>\r\n          </el-row>\r\n        </el-card>\r\n      </div>\r\n\r\n      <!-- 财务数据表格 -->\r\n      <div class=\"finance-tables\">\r\n        <el-row :gutter=\"20\">\r\n          <el-col :span=\"12\">\r\n            <el-card>\r\n              <div slot=\"header\">\r\n                <span>最近交易</span>\r\n                <el-button style=\"float: right; padding: 3px 0\" type=\"text\" @click=\"viewAllTransactions\">\r\n                  查看全部\r\n                </el-button>\r\n              </div>\r\n              <el-table :data=\"recentTransactions\" size=\"small\" max-height=\"300\">\r\n                <el-table-column prop=\"orderNo\" label=\"订单号\" width=\"120\" />\r\n                <el-table-column prop=\"amount\" label=\"金额\" width=\"80\">\r\n                  <template slot-scope=\"scope\">\r\n                    ¥{{ scope.row.amount }}\r\n                  </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"type\" label=\"类型\" width=\"80\">\r\n                  <template slot-scope=\"scope\">\r\n                    <el-tag size=\"mini\" :type=\"scope.row.type === '收入' ? 'success' : 'warning'\">\r\n                      {{ scope.row.type }}\r\n                    </el-tag>\r\n                  </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"time\" label=\"时间\" />\r\n              </el-table>\r\n            </el-card>\r\n          </el-col>\r\n          <el-col :span=\"12\">\r\n            <el-card>\r\n              <div slot=\"header\">\r\n                <span>待处理结算</span>\r\n                <el-button style=\"float: right; padding: 3px 0\" type=\"text\" @click=\"viewAllSettlements\">\r\n                  查看全部\r\n                </el-button>\r\n              </div>\r\n              <el-table :data=\"pendingSettlements\" size=\"small\" max-height=\"300\">\r\n                <el-table-column prop=\"teacherName\" label=\"教师\" width=\"80\" />\r\n                <el-table-column prop=\"amount\" label=\"金额\" width=\"80\">\r\n                  <template slot-scope=\"scope\">\r\n                    ¥{{ scope.row.amount }}\r\n                  </template>\r\n                </el-table-column>\r\n                <el-table-column prop=\"status\" label=\"状态\" width=\"80\">\r\n                  <template slot-scope=\"scope\">\r\n                    <el-tag size=\"mini\" type=\"warning\">{{ scope.row.status }}</el-tag>\r\n                  </template>\r\n                </el-table-column>\r\n                <el-table-column label=\"操作\">\r\n                  <template slot-scope=\"scope\">\r\n                    <el-button size=\"mini\" type=\"text\" @click=\"processSettlement(scope.row)\">\r\n                      处理\r\n                    </el-button>\r\n                  </template>\r\n                </el-table-column>\r\n              </el-table>\r\n            </el-card>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n    </div>\r\n  </Layout>\r\n</template>\r\n\r\n<script>\r\nimport Layout from '@/components/Layout.vue'\r\n\r\nexport default {\r\n  name: 'Finance',\r\n  components: {\r\n    Layout\r\n  },\r\n  data() {\r\n    return {\r\n      financeData: {\r\n        totalRevenue: '1,256,890.50',\r\n        totalCommission: '251,378.10',\r\n        netProfit: '1,005,512.40',\r\n        pendingSettlement: '45,680.20'\r\n      },\r\n      recentTransactions: [\r\n        {\r\n          orderNo: 'ORD20240116001',\r\n          amount: '299.00',\r\n          type: '收入',\r\n          time: '2024-01-16 14:30'\r\n        },\r\n        {\r\n          orderNo: 'ORD20240116002',\r\n          amount: '59.80',\r\n          type: '佣金',\r\n          time: '2024-01-16 14:25'\r\n        },\r\n        {\r\n          orderNo: 'ORD20240116003',\r\n          amount: '499.00',\r\n          type: '收入',\r\n          time: '2024-01-16 14:20'\r\n        }\r\n      ],\r\n      pendingSettlements: [\r\n        {\r\n          teacherName: '李老师',\r\n          amount: '1,580.50',\r\n          status: '待结算',\r\n          id: 1\r\n        },\r\n        {\r\n          teacherName: '王老师',\r\n          amount: '2,340.80',\r\n          status: '待结算',\r\n          id: 2\r\n        },\r\n        {\r\n          teacherName: '张老师',\r\n          amount: '980.30',\r\n          status: '待结算',\r\n          id: 3\r\n        }\r\n      ]\r\n    }\r\n  },\r\n  methods: {\r\n    exportFinanceData() {\r\n      this.$message.info('财务报表导出功能开发中...')\r\n    },\r\n    \r\n    refreshData() {\r\n      this.$message.success('财务数据已刷新')\r\n    },\r\n    \r\n    handleSettlement() {\r\n      this.$message.info('佣金结算功能开发中...')\r\n    },\r\n    \r\n    handleRefund() {\r\n      this.$message.info('退款处理功能开发中...')\r\n    },\r\n    \r\n    handleInvoice() {\r\n      this.$message.info('发票管理功能开发中...')\r\n    },\r\n    \r\n    handleTax() {\r\n      this.$message.info('税务管理功能开发中...')\r\n    },\r\n    \r\n    viewAllTransactions() {\r\n      this.$message.info('查看全部交易功能开发中...')\r\n    },\r\n    \r\n    viewAllSettlements() {\r\n      this.$message.info('查看全部结算功能开发中...')\r\n    },\r\n    \r\n    processSettlement(settlement) {\r\n      this.$confirm(`确定要处理 ${settlement.teacherName} 的结算吗？`, '确认结算', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning'\r\n      }).then(() => {\r\n        this.$message.success('结算处理成功')\r\n      }).catch(() => {})\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.finance-container {\r\n  padding: 20px;\r\n  background-color: #f5f7fa;\r\n  min-height: 100vh;\r\n}\r\n\r\n.page-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  margin-bottom: 20px;\r\n  padding: 20px;\r\n  background: linear-gradient(135deg, #2c5aa0 0%, #1e3d72 100%);\r\n  border-radius: 10px;\r\n  color: white;\r\n}\r\n\r\n.header-content h1 {\r\n  margin: 0 0 5px 0;\r\n  font-size: 24px;\r\n  font-weight: 600;\r\n}\r\n\r\n.header-content p {\r\n  margin: 0;\r\n  opacity: 0.9;\r\n  font-size: 14px;\r\n}\r\n\r\n.page-title i {\r\n  margin-right: 8px;\r\n  color: #ffd04b;\r\n}\r\n\r\n.finance-stats {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fit, minmax(250px, 1fr));\r\n  gap: 20px;\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.stat-card {\r\n  background: white;\r\n  border-radius: 10px;\r\n  padding: 24px;\r\n  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.1);\r\n  display: flex;\r\n  align-items: center;\r\n  transition: transform 0.3s ease;\r\n}\r\n\r\n.stat-card:hover {\r\n  transform: translateY(-5px);\r\n}\r\n\r\n.stat-icon {\r\n  width: 60px;\r\n  height: 60px;\r\n  border-radius: 10px;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  margin-right: 16px;\r\n  font-size: 24px;\r\n  color: white;\r\n}\r\n\r\n.stat-card.revenue .stat-icon {\r\n  background: linear-gradient(135deg, #27ae60 0%, #2ecc71 100%);\r\n}\r\n\r\n.stat-card.commission .stat-icon {\r\n  background: linear-gradient(135deg, #f39c12 0%, #e67e22 100%);\r\n}\r\n\r\n.stat-card.profit .stat-icon {\r\n  background: linear-gradient(135deg, #e74c3c 0%, #c0392b 100%);\r\n}\r\n\r\n.stat-card.pending .stat-icon {\r\n  background: linear-gradient(135deg, #3498db 0%, #2980b9 100%);\r\n}\r\n\r\n.stat-content {\r\n  flex: 1;\r\n}\r\n\r\n.stat-value {\r\n  font-size: 28px;\r\n  font-weight: 700;\r\n  color: #2c3e50;\r\n  line-height: 1;\r\n  margin-bottom: 8px;\r\n}\r\n\r\n.stat-label {\r\n  font-size: 14px;\r\n  color: #7f8c8d;\r\n  margin-bottom: 4px;\r\n}\r\n\r\n.stat-trend {\r\n  display: flex;\r\n  align-items: center;\r\n  font-size: 12px;\r\n  color: #27ae60;\r\n}\r\n\r\n.finance-operations {\r\n  margin-bottom: 30px;\r\n}\r\n\r\n.operation-card {\r\n  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.operation-item {\r\n  display: flex;\r\n  align-items: center;\r\n  padding: 20px;\r\n  border-radius: 8px;\r\n  cursor: pointer;\r\n  transition: background-color 0.3s ease;\r\n}\r\n\r\n.operation-item:hover {\r\n  background-color: #f8f9fa;\r\n}\r\n\r\n.operation-icon {\r\n  width: 40px;\r\n  height: 40px;\r\n  border-radius: 8px;\r\n  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  margin-right: 12px;\r\n}\r\n\r\n.operation-icon i {\r\n  font-size: 18px;\r\n  color: white;\r\n}\r\n\r\n.operation-text h4 {\r\n  margin: 0 0 4px 0;\r\n  font-size: 14px;\r\n  color: #2c3e50;\r\n}\r\n\r\n