mes = {\r\n        competition: '竞赛',\r\n        regular: '常规',\r\n        mock: '模拟'\r\n      }\r\n      return names[type] || '其他'\r\n    },\r\n    \r\n    getStatusColor(status) {\r\n      const colors = {\r\n        pending: 'info',\r\n        active: 'success',\r\n        completed: 'warning'\r\n      }\r\n      return colors[status] || 'info'\r\n    },\r\n    \r\n    getStatusName(status) {\r\n      const names = {\r\n        pending: '未开始',\r\n        active: '进行中',\r\n        completed: '已结束'\r\n      }\r\n      return names[status] || '未知'\r\n    },\r\n    \r\n    formatDateTime(datetime) {\r\n      if (!datetime) return '-'\r\n      return new Date(datetime).toLocaleString('zh-CN')\r\n    }\r\n  },\r\n  \r\n  mounted() {\r\n    this.loadExams()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.exams-container {\r\n  padding: 0;\r\n}\r\n\r\n.page-header {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: flex-start;\r\n  margin-bottom: 24px;\r\n  padding: 24px;\r\n  background: #ffffff;\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.header-content {\r\n  flex: 1;\r\n}\r\n\r\n.page-title {\r\n  margin: 0 0 8px 0;\r\n  font-size: 24px;\r\n  font-weight: 600;\r\n  color: #2c3e50;\r\n}\r\n\r\n.page-description {\r\n  margin: 0;\r\n  color: #7f8c8d;\r\n  font-size: 14px;\r\n}\r\n\r\n.header-actions {\r\n  flex-shrink: 0;\r\n}\r\n\r\n.page-content {\r\n  background: #f8f9fa;\r\n}\r\n\r\n.content-card {\r\n  border-radius: 8px;\r\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.filter-section {\r\n  padding: 20px;\r\n  background: #fafbfc;\r\n  border-bottom: 1px solid #e9ecef;\r\n  border-radius: 8px 8px 0 0;\r\n}\r\n\r\n.filter-form {\r\n  margin: 0;\r\n}\r\n\r\n.table-section {\r\n  padding: 0;\r\n}\r\n\r\n.exam-info {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 4px;\r\n}\r\n\r\n.exam-name {\r\n  font-weight: 600;\r\n  color: #2c3e50;\r\n}\r\n\r\n.exam-subject {\r\n  font-si