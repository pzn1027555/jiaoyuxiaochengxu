ct }}</div>\r\n                </div>\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"type\" label=\"考试类型\" width=\"120\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag :type=\"getExamTypeColor(scope.row.type)\">\r\n                  {{ getExamTypeName(scope.row.type) }}\r\n                </el-tag>\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"startTime\" label=\"开始时间\" width=\"160\" sortable=\"custom\">\r\n              <template slot-scope=\"scope\">\r\n                {{ formatDateTime(scope.row.startTime) }}\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"duration\" label=\"考试时长\" width=\"100\">\r\n              <template slot-scope=\"scope\">\r\n                {{ scope.row.duration }}分钟\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"participants\" label=\"参与人数\" width=\"100\">\r\n              <template slot-scope=\"scope\">\r\n                {{ scope.row.participants || 0 }}人\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column prop=\"status\" label=\"状态\" width=\"100\">\r\n              <template slot-scope=\"scope\">\r\n                <el-tag :type=\"getStatusColor(scope.row.status)\">\r\n                  {{ getStatusName(scope.row.status) }}\r\n                </el-tag>\r\n              </template>\r\n            </el-table-column>\r\n            \r\n            <el-table-column label=\"操作\" width=\"200\" fixed=\"right\">\r\n              <template slot-scope=\"scope\">\r\n                <el-button type=\"text\" size=\"small\" @click=\"viewExam(scope.row)\">\r\n                  查看\r\n                </el-button>\r\n                <el-button type=\"text\" size=\"small\" @click=\"editExam(scope.row)\">\r\n                  编辑\r\n                </el-button>\r\n                <el-button type=\"text\" size=\"small\" @click=\"viewResults(scope.row)\">\r\n                  成绩\r\n                </el-button>\r\n                <el-button \r\n                  type=\"text\" \r\n                  size=\"small\" \r\n                  style=\"color: #f56c6c;\" \r\n                  @click=\"deleteExam(scope.row)\"\r\n                >\r\n                  删除\r\n                </el-button>\r\n              </template>\r\n            </el-table-column>\r\n          </el-table>\r\n          \r\n          <div class=\"pagination-section\">\r\n            <el-pagination\r\n              @size-change=\"handleSizeChange\"\r\n              @current-change=\"handleCurrentChange\"\r\n              :current-page=\"pagination.current\"\r\n              :page-sizes=\"[10, 20, 50, 100]\"\r\n              :page-size=\"pagination.size\"\r\n              layout=\"total, sizes, prev, pager, next, jumper\"\r\n              :total=\"pagination.total\"\r\n            />\r\n          </div>\r\n        </div>\r\n      </el-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'Exams',\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      filterForm: {\r\n        name: '',\r\n        status: ''\r\n      },\r\n      examsList: [\r\n        {\r\n          id: 1,\r\n          name: '数学竞赛初赛',\r\n          subject: '数学',\r\n          type: 'competition',\r\n          startTime: '2024-01-15 09:00:00',\r\n          duration: 120,\r\n          participants: 45,\r\n          status: 'completed'\r\n        },\r\n        {\r\n          id: 2,\r\n          name: '物理期中考试',\r\n          subject: '物理',\r\n          type: 'regular',\r\n          startTime: '2024-01-20 14:00:00',\r\n          duration: 90,\r\n          participants: 32,\r\n          status: 'active'\r\n        },\r\n        {\r\n          id: 3,\r\n          name: 'IGCSE数学模拟考',\r